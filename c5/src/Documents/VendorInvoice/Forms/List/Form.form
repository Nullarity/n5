    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Documents.Ref, Documents.DeletionMark, Documents.Number,	Documents.Date,	Documents.Posted,Documents.Amount, Documents.CloseAdvances, Documents.Company, Documents.Contract, Documents.Creator,
	Documents.Currency,	Documents.Department, Documents.Discount, Documents.ExpensesPeriod, Documents.Factor, Documents.GrossAmount,	Documents.Import, Documents.Manager, Documents.Memo, 
	Documents.PaymentDate, Documents.PaymentOption,	Documents.Prices, Documents.PurchaseOrder, Documents.Rate, Documents.Reference,	Documents.ReferenceDate, Documents.Tax,	Documents.Taxable, Documents.TaxableAmount,
	Documents.TaxCode, Documents.TaxGroup, Documents.TaxRate, Documents.Vendor,	Documents.VendorAccount, Documents.Warehouse, Documents.Accounts, Documents.Discounts, Documents.FixedAssets, Documents.IntangibleAssets,
	Documents.Items, Documents.Services, Documents.Taxes, Documents.PointInTime,
	case Forms.Status
		when value ( Enum.FormStatuses.Saved ) then 1
		when value ( Enum.FormStatuses.Printed ) then 2
		when value ( Enum.FormStatuses.Canceled ) then 3
		when value ( Enum.FormStatuses.Submitted ) then 4
		when value ( Enum.FormStatuses.Waiting ) then 5
		when value ( Enum.FormStatuses.Unloaded ) then 6
		else 0
	end as Status,
	Documents.Posted and isnull ( Debts.AmountBalance, 0 ) = 0 as Paid,
	case when Documents.ContractAmount = 0 then 0
		when Documents.Posted then cast ( ( Documents.ContractAmount - isnull ( PaymentDiscounts.AmountTurnover, 0 ) - Debts.AmountBalance )
			* 100 / ( Documents.ContractAmount - isnull ( PaymentDiscounts.AmountTurnover, 0 ) ) as Number ( 15 ) )
	else 0
	end as PaidPercent,
	TasksList.Progress as TaskProgress, TasksList.Completed as TaskCompleted, TasksList.Status as TaskStatus, TasksList.Task as TaskRef, TasksList.Observation as Observation
from Document.VendorInvoice as Documents
	//
	// Forms
	//
	{left join InformationRegister.Forms as Forms
	on Forms.Document = Documents.Ref}
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as TasksList
	on TasksList.Source = Documents.Ref
	and TasksList.User = &amp;Performer}
	//
	// Debts
	//
	left join AccumulationRegister.VendorInvoiceDebts.Balance as Debts
	on Debts.Document = Documents.Ref	
	//
	// Discounts
	//
	left join AccumulationRegister.Discounts.Turnovers as PaymentDiscounts
	on PaymentDiscounts.Document = Documents.Ref</queryText>
      <mainTable>Document.VendorInvoice</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
