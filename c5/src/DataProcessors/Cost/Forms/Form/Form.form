  <attributes>
    <name>Boundaries</name>
    <title>
      <key>en</key>
      <value>Boundaries</value>
    </title>
    <id>2</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Boundaries.Item as Item, Boundaries.Item.Code as Code, Boundaries.Period as Period, Boundaries.Actual as Actual
from (
	select distinct Boundaries.Item as Item, Boundaries.Period as Period,
			case when Irrelevant.Item is null then true else false end as Actual
	from Sequence.Cost.Boundaries as Boundaries
		//
		// Irrelevant Items
		//
		left join Sequence.Cost as Irrelevant
		on Irrelevant.Company = &amp;Company
		and Irrelevant.Item = Boundaries.Item
		and Irrelevant.PointInTime > Boundaries.PointInTime
		and Irrelevant.Period &lt;= &amp;Bound
	where Boundaries.Company = &amp;Company
	union
	select Cost.Item, min ( Cost.Period ), false
	from Sequence.Cost as Cost
	where Cost.Company = &amp;Company
	and Cost.Item not in ( select distinct Item from Sequence.Cost.Boundaries where Company = &amp;Company )
	group by Cost.Item
) as Boundaries
</queryText>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
