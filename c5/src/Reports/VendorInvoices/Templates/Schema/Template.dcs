		<dataSource>DataSource1</dataSource>
		<query>// Items
select allowed value ( Enum.Tables.Items ) as Table, Items.Ref as Ref, Items.LineNumber as Row, Items.Item as Item,
	Items.Feature as Feature, Items.Series as Series, Items.Account as Account,
	case when Items.Item.CountPackages then Items.QuantityPkg else Items.Quantity end as Quantity,
	case when Items.Item.CountPackages then Items.Package else value ( Catalog.Packages.EmptyRef ) end as Package,
	case when ( Items.Warehouse = value ( Catalog.Warehouses.EmptyRef ) ) then Items.Ref.Warehouse else Items.Warehouse end as Warehouse
into Items
from Document.VendorInvoice.Items as Items
{where Items.Ref.* as Ref, Items.Ref.Date between &amp;DateStart and &amp;DateEnd, Items.Item.* as Item,
	case when ( Items.Warehouse = value ( Catalog.Warehouses.EmptyRef ) ) then Items.Ref.Warehouse else Items.Warehouse end as Warehouse,
	Items.Feature.* as Feature, Items.Series.* as Series, Items.Account.* as Account,
	value ( Enum.Tables.Items ) as Table
}
union all
select value ( Enum.Tables.FixedAssets ), Items.Ref, Items.LineNumber, Items.Item, null, null,
	Items.Item.Account, 1, Items.Item.Unit, Items.Ref.Warehouse
from Document.VendorInvoice.FixedAssets as Items
{where Items.Ref.* as Ref, Items.Ref.Date between &amp;DateStart and &amp;DateEnd, Items.Item.* as Item,
	Items.Ref.Warehouse as Warehouse, Items.Item.Account.* as Account,
	value ( Enum.Tables.FixedAssets ) as Table}
union all
select value ( Enum.Tables.IntangibleAssets ), Items.Ref, Items.LineNumber, Items.Item, null, null,
	Items.Item.Account, 1, Items.Item.Unit, Items.Ref.Warehouse
from Document.VendorInvoice.IntangibleAssets as Items
{where Items.Ref.* as Ref, Items.Ref.Date between &amp;DateStart and &amp;DateEnd, Items.Item.* as Item,
	Items.Ref.Warehouse as Warehouse, Items.Item.Account.* as Account,
	value ( Enum.Tables.IntangibleAssets ) as Table}
;
// Documents
select distinct Items.Ref as Ref
into Documents
from Items as Items
;
select min ( Items.Row ) as Row, value ( Enum.Tables.Items ) as Table, Items.Ref as Ref,
	ItemDetails.ItemKey as ItemKey, sum ( Items.Quantity ) as Quantity
into ItemKeys
from Items as Items
	//
	// ItemDetails
	//
	join InformationRegister.ItemDetails as ItemDetails
	on ItemDetails.Item = Items.Item
	and ItemDetails.Feature = Items.Feature
	and ItemDetails.Package = Items.Package
	and ItemDetails.Warehouse = Items.Warehouse
	and ItemDetails.Series = Items.Series
	and ItemDetails.Account = Items.Account
where Items.Table = value ( Enum.Tables.Items )
group by Items.Ref, ItemDetails.ItemKey
union all
select Items.Row, Items.Table, Items.Ref, Items.Item, 1
from Items as Items
where Items.Table in (
	value ( Enum.Tables.FixedAssets ),
	value ( Enum.Tables.IntangibleAssets )
)
;
select Items.Ref as Ref, Items.Item as Item, Items.Feature as Feature,
	Items.Series as Series, Items.Account as Account, Items.Table as Table,
	case Items.Package when value ( Catalog.Packages.EmptyRef ) then Items.Item.Unit else Items.Package end as Package,
	Items.Warehouse as Warehouse, Items.Expense as Expense, Items.Amount as ExpenseAmount,
	ItemKeys.ItemKey as ItemKey, ItemKeys.Quantity as Quantity,
	case Items.Table
		when value ( Enum.Tables.Items ) then 0
		when value ( Enum.Tables.FixedAssets ) then 1
		else 2
	end * 100000 + Items.Row as Row
{select Ref.*, Table.*, Row, Item.*, Feature.*, Series.*, Account.*, Package.*, Warehouse.*, Expense.*,
	ExpenseAmount, ItemKey, Quantity}
from (
	select min ( Items.Row ) as Row, Items.Table as Table, Items.Ref as Ref, Items.Item as Item, Items.Feature as Feature,
		Items.Series as Series, Items.Account as Account, Items.Package as Package,
		Items.Warehouse as Warehouse, Expenses.Expense as Expense, sum ( Expenses.Amount ) as Amount
	from Items as Items
	    //
	    // Expenses
	    //
		left join (
			select value ( Enum.Tables.Items ) as Table, Expenses.Document as Document,
				Charges.Charge as Expense, Expenses.DocumentRow as Row, Expenses.Amount as Amount
			from Document.CustomsDeclaration.Charges as Charges
				//
				// Expenses
				//
				join InformationRegister.ItemExpenses as Expenses
				on Expenses.DocumentTable = value ( Enum.Tables.Items )
				and Expenses.Source = Charges.Ref
				and Expenses.Table = value ( Enum.Tables.Charges )
				and Expenses.TableRow = Charges.LineNumber
				//
				// Documents
				//
				join Documents as Documents
				on Documents.Ref = Expenses.Document
			union all
			select Expenses.DocumentTable, Expenses.Document, Services.Item, Expenses.DocumentRow,
				Expenses.Amount
			from Document.VendorInvoice.Services as Services
				//
				// Expenses
				//
				join InformationRegister.ItemExpenses as Expenses
				on Expenses.Source = Services.Ref
				and Expenses.Table = value ( Enum.Tables.Services )
				and Expenses.TableRow = Services.LineNumber
				//
				// Documents
				//
				join Documents as Documents
				on Documents.Ref = Expenses.Document
		) as Expenses
		on Expenses.Document = Items.Ref
		and Expenses.Table = Items.Table
		and Expenses.Row = Items.Row
	group by Items.Ref, Items.Table, Items.Item, Items.Feature, Items.Series, Items.Account,
		Items.Package, Items.Warehouse, Expenses.Expense
	) as Items
	//
	// ItemKeys
	//
	join ItemKeys as ItemKeys
	on ItemKeys.Table = Items.Table
	and ItemKeys.Row = Items.Row
	and ItemKeys.Ref = Items.Ref
{where Items.Expense.* as Expense, Items.Amount as ExpenseAmount, ItemKeys.Quantity as Quantity}
</query>
