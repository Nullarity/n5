		<dataSource>DataSource1</dataSource>
		<query>select cast ( Cost.Recorder as Document.VendorInvoice ) as CostRecorder,
	Cost.ItemKey as CostItemKey, sum ( Cost.Amount ) as CostTotal,
	sum ( case Cost.Dependency when undefined then Cost.Amount else 0 end ) as CostBase,
	cast ( case sum ( Cost.Quantity ) when 0 then 0 else sum ( Cost.Amount ) / sum ( Cost.Quantity ) end as Number ( 15, 2 ) ) as CostUnit
{select CostRecorder.*, CostItemKey, CostTotal, CostBase, CostUnit}
from AccumulationRegister.Cost as Cost
where Cost.Recorder in ( &amp;Recorders )
group by Cost.Recorder, Cost.ItemKey
union all
select cast ( Cost.Recorder as Document.VendorInvoice ), Cost.ExtDimensionDr1,
	sum ( Cost.Amount ) as CostTotal,
	sum ( case Cost.Operation when value ( Enum.Operations.AdditionalExpenses ) then 0 else Cost.Amount end ),
	sum ( Cost.Amount )
from AccountingRegister.General.RecordsWithExtDimensions ( , ,
	Recorder in ( &amp;Recorders )
	and valuetype ( ExtDimensionDr1 ) in (
		type ( Catalog.FixedAssets ),
		type ( Catalog.IntangibleAssets )
		)
	) as Cost
group by Cost.Recorder, Cost.ExtDimensionDr1
</query>
