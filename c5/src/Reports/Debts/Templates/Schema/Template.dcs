		<dataSource>DataSource1</dataSource>
		<query>// CurrencyRates
select Rates.Currency as Currency, Rates.Rate as Rate, Rates.Factor as Factor
into Rates
from InformationRegister.ExchangeRates.SliceLast ( , &amp;LocalCurrency ) as Rates
;
//
// Main selection
//
select &amp;StrTotalsGroup as StrTotalsGroup, &amp;StrPayment as StrPayment, &amp;StrOverduePayment as StrOverduePayment,
	Balances.PaymentKey as PaymentKey, Balances.Contract.Owner as Customer,
	Balances.Contract as Contract, Balances.Document as Document, Balances.Detail as Detail,
	PaymentDetails.Option as Option,
	case when &amp;OperationDate then Balances.Document.Date else PaymentDetails.Date end as PaymentDate,
	datediff ( case when &amp;OperationDate then Balances.Document.Date else PaymentDetails.Date end,
		&amp;DebtsOnDate, day ) as BalanceDays,
	Intervals.Presentation as OverduePeriods, Intervals.LineNumber as Interval, Intervals.LineNumber is null as CurrentDebt,
	case when &amp;LocalCurrency then Balances.PaymentBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.PaymentBalance
	end as PaymentBalance,
	case when &amp;LocalCurrency then Balances.AmountBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.AmountBalance
	end as AmountDebt,
	case when &amp;LocalCurrency then Balances.OverpaymentBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.OverpaymentBalance
	end as Overpayment
	{select StrTotalsGroup, StrPayment, StrOverduePayment, Customer.* as Customer, Contract.*, Document.*, Detail.*, PaymentDate, PaymentBalance, BalanceDays,
	OverduePeriods, AmountDebt, Overpayment, PaymentKey, Option.*, Interval, CurrentDebt}
from AccumulationRegister.Debts.Balance ( &amp;Asof, {Contract.Company.* as Company, Contract.Owner.* as Customer, Contract.* as Contract, Document.* as Document,
	Detail.* as Detail, Contract.Currency.* as Currency} ) as Balances
	//
	// PaymentDetails
	//
	left join InformationRegister.PaymentDetails as PaymentDetails
	on PaymentDetails.PaymentKey = Balances.PaymentKey
	//
	// Rates
	//
	left join Rates as Rates
	on Rates.Currency = Balances.Contract.Currency
	//
	// LocalCurrency
	//
	left join Constants as Constants
	on true
	//
	// Intervals
	//
	left join Catalog.Intervals.Intervals as Intervals
	on Intervals.Ref = &amp;Interval
	and datediff ( case when &amp;OperationDate then Balances.Document.Date else PaymentDetails.Date end,
		&amp;DebtsOnDate, day ) between Intervals.Start and Intervals.Finish
{where Balances.PaymentBalance as PaymentBalance, PaymentDetails.Option.* as Option,
	case when &amp;OperationDate then Balances.Document.Date else PaymentDetails.Date end as PaymentDate,
	case when &amp;LocalCurrency then Balances.AmountBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.AmountBalance
	end as AmountDebt,
	case when &amp;LocalCurrency then Balances.OverpaymentBalance * isnull ( Rates.Rate, 1 ) / isnull ( Rates.Factor, 1 )
		else Balances.OverpaymentBalance
	end as Overpayment,
	datediff ( case when &amp;OperationDate then Balances.Document.Date else PaymentDetails.Date end,
		&amp;DebtsOnDate, day ) as BalanceDays,
	Intervals.LineNumber is null as CurrentDebt}
</query>
