		<dataSource>DataSource1</dataSource>
		<query>//
// TimeEntries
//
select allowed dateadd ( beginofperiod ( TimeEntries.Ref.Date, day ), minute, hour ( TimeEntries.TimeStart ) * 60 + minute ( TimeEntries.TimeStart ) ) as Date,
	TimeEntries.Ref as TimeEntry, TimeEntries.Ref.Project as Project, TimeEntries.Ref.Project.Amount as ProjectAmount, TimeEntries.Ref.Employee as Employee,
	TimeEntries.Ref.Customer as Customer, TimeEntries.Task as Task, TimeEntries.Description as Description, TimeEntries.TimeType as TimeType, TimeEntries.Minutes as Minutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Billable ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as BillableMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.NonBillable ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as NonBillableMinutes,
	case when ( TimeEntries.TimeType in ( value ( Enum.Time.Vacation ), value ( Enum.Time.ExtendedVacation ) ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as VacationMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Banked ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as BankedMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.BankedUse ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as BankedUseMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.DayOff ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as DayOffMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Overtime ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as OvertimeMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Evening ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as EveningMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Night ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as NightMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Sickness ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as SicknessMinutes,
	case when ( TimeEntries.TimeType = value ( Enum.Time.Holiday ) ) and ( RejectedTimeEntries.TimeEntry is null ) then TimeEntries.Minutes else 0 end as HolidayMinutes,
	isnull ( TimesheetStatuses.Status, value ( Enum.TimesheetStatuses.None ) ) as TimesheetsStatus,
	case when RejectedTimeEntries.TimeEntry is null then false else true end as TimeRejected,
	case when RejectedTimeEntries.TimeEntry is null then 0 else TimeEntries.Minutes end as RejectedMinutes,
	Timesheets.Ref as Timesheet, TimeEntries.TimeStart as TimeStart, TimeEntries.TimeEnd as TimeEnd
{select Date, TimeEntry.*, Project.* as Project, ProjectAmount as ProjectAmount, Employee.* as Employee, Customer.*, Task.*, Description, TimeType, Minutes, BillableMinutes,
	NonBillableMinutes, VacationMinutes, SicknessMinutes, HolidayMinutes, TimesheetsStatus, TimeRejected, RejectedMinutes, Timesheet.*,
	TimeStart, TimeEnd, BankedMinutes, BankedUseMinutes, OvertimeMinutes, EveningMinutes, NightMinutes, DayOffMinutes}
from Document.TimeEntry.Tasks as TimeEntries
	//
	// Timesheets
	//
	left join Document.Timesheet as Timesheets
	on Timesheets.Employee = TimeEntries.Ref.Employee
	and TimeEntries.Ref.Date between Timesheets.DateStart and endofperiod ( Timesheets.DateEnd, day )
	and not Timesheets.DeletionMark
	//
	// TimesheetStatuses
	//
	left join InformationRegister.TimesheetStatuses as TimesheetStatuses
	on TimesheetStatuses.Timesheet = Timesheets.Ref
	//
	// RejectedTimeEntries
	//
	left join InformationRegister.RejectedTimeEntries as RejectedTimeEntries
	on RejectedTimeEntries.TimeEntry = TimeEntries.Ref
where TimeEntries.Ref.Posted
{where TimeEntries.Ref.Date between &amp;DateStart and &amp;DateEnd,
	TimeEntries.Ref.Project.* as Project, TimeEntries.Ref.Employee.* as Employee,
	TimeEntries.Ref.Customer.* as Customer, TimeEntries.Task.* as Task, TimeEntries.TimeType as TimeType,
	Timesheets.Ref.* as Timesheet, TimeEntries.Description as Description,
	isnull ( TimesheetStatuses.Status, value ( Enum.TimesheetStatuses.None ) ) as TimesheetsStatus,
	case when RejectedTimeEntries.TimeEntry is null then false else true end as TimeRejected}
union all
//
// Timesheets
//
select Timesheets.Date, null, Timesheets.Project, Timesheets.Project.Amount, Timesheets.Employee, Timesheets.Customer, null, null,
	Timesheets.TimeType, Timesheets.Minutes,
	case when Timesheets.TimeType = value ( Enum.Time.Billable ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.NonBillable ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType in ( value ( Enum.Time.Vacation ), value ( Enum.Time.ExtendedVacation ) ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.Banked ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.BankedUse ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.DayOff ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.Overtime ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.Evening ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.Night ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.Sickness ) then Timesheets.Minutes else 0 end,
	case when Timesheets.TimeType = value ( Enum.Time.Holiday ) then Timesheets.Minutes else 0 end,
	isnull ( TimesheetStatuses.Status, value ( Enum.TimesheetStatuses.None ) ),
	case when RejectedTimesheetRecords.Resolution is null then false else true end,
	case when RejectedTimesheetRecords.Resolution is null then 0 else Timesheets.Minutes end,
	Timesheets.Timesheet, null, null
from (
		//
		// Timesheets.Month
		//
		select Timesheets.Ref.Date as Date, Timesheets.Project as Project, Timesheets.Ref.Employee as Employee, Timesheets.Customer as Customer,
			Timesheets.TimeType as TimeType, Timesheets.Ref as Timesheet, Timesheets.TotalMinutesByTimesheet as Minutes,
			Timesheets.TimesheetApproval as TimesheetApproval
		from Document.Timesheet.Month as Timesheets
		where Timesheets.Ref.Posted
		and Timesheets.TotalMinutesByTimesheet &lt;&gt; 0
		{where Timesheets.Ref.Date between &amp;DateStart and &amp;DateEnd, Timesheets.Project.* as Project, Timesheets.Ref.Employee.* as Employee,
			Timesheets.Customer.* as Customer, Timesheets.TimeType as TimeType, Timesheets.Ref.* as Timesheet}
		union all
		//
		// Timesheets.OneWeek
		//
		select Timesheets.Ref.Date, Timesheets.Project, Timesheets.Ref.Employee, Timesheets.Customer,
			Timesheets.TimeType, Timesheets.Ref, Timesheets.TotalMinutesByTimesheet, Timesheets.TimesheetApproval
		from Document.Timesheet.OneWeek as Timesheets
		where Timesheets.Ref.Posted
		and Timesheets.TotalMinutesByTimesheet &lt;&gt; 0
		{where Timesheets.Ref.Date between &amp;DateStart and &amp;DateEnd, Timesheets.Project.* as Project, Timesheets.Ref.Employee.* as Employee,
			Timesheets.Customer.* as Customer, Timesheets.TimeType as TimeType, Timesheets.Ref.* as Timesheet}
		union all
		//
		// Timesheets.Other
		//
		select Timesheets.Ref.Date, Timesheets.Project, Timesheets.Ref.Employee, Timesheets.Customer,
			Timesheets.TimeType, Timesheets.Ref, Timesheets.TotalMinutesByTimesheet, Timesheets.TimesheetApproval
		from Document.Timesheet.Other as Timesheets
		where Timesheets.Ref.Posted
		and Timesheets.TotalMinutesByTimesheet &lt;&gt; 0
		{where Timesheets.Ref.Date between &amp;DateStart and &amp;DateEnd, Timesheets.Project.* as Project, Timesheets.Ref.Employee.* as Employee,
			Timesheets.Customer.* as Customer, Timesheets.TimeType as TimeType, Timesheets.Ref.* as Timesheet}
		union all
		//
		// Timesheets.TwoWeeks
		//
		select Timesheets.Ref.Date, Timesheets.Project, Timesheets.Ref.Employee, Timesheets.Customer,
			Timesheets.TimeType, Timesheets.Ref, Timesheets.TotalMinutesByTimesheet, Timesheets.TimesheetApproval
		from Document.Timesheet.TwoWeeks as Timesheets
		where Timesheets.Ref.Posted
		and Timesheets.TotalMinutesByTimesheet &lt;&gt; 0
		{where Timesheets.Ref.Date between &amp;DateStart and &amp;DateEnd, Timesheets.Project.* as Project, Timesheets.Ref.Employee.* as Employee,
			Timesheets.Customer.* as Customer, Timesheets.TimeType as TimeType, Timesheets.Ref.* as Timesheet}
	) as Timesheets
	//
	// TimesheetStatuses
	//
	left join InformationRegister.TimesheetStatuses as TimesheetStatuses
	on TimesheetStatuses.Timesheet = Timesheets.Timesheet
	//
	// RejectedTimesheetRecords
	//
	left join InformationRegister.TimeResolutions as RejectedTimesheetRecords
	on RejectedTimesheetRecords.TimesheetApproval = Timesheets.TimesheetApproval
	and RejectedTimesheetRecords.Resolution = value ( Enum.Resolutions.Reject )
	and TimesheetStatuses.Status = value ( Enum.TimesheetStatuses.Finished )
{where isnull ( TimesheetStatuses.Status, value ( Enum.TimesheetStatuses.None ) ) as TimesheetsStatus,
	case when RejectedTimesheetRecords.Resolution is null then false else true end as TimeRejected}
</query>
