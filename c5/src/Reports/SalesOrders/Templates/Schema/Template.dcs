		<dataSource>DataSource1</dataSource>
		<query>//
// Debts
//
select allowed cast ( Debts.Document as Document.SalesOrder ) as SalesOrder,
	sum ( Debts.AmountBalance ) as Amount, sum ( Debts.PaymentBalance ) as Debt,
	sum ( Debts.OverpaymentBalance ) as Overpayment
into Debts
from AccumulationRegister.Debts.Balance ( , {Contract.Company.* as Company, Contract.* as Contract, Contract.Owner.* as Customer,
	cast ( Document as Document.SalesOrder ).* as SalesOrder} ) as Debts
where Debts.Document refs Document.SalesOrder
group by Debts.Document
index by cast ( Debts.Document as Document.SalesOrder )
;
//
// Main selection
//
select allowed Debts.SalesOrder as SalesOrder, Debts.SalesOrder.Customer as Customer,
	Debts.SalesOrder.Contract as Contract, Debts.SalesOrder.Amount as Amount,
	Debts.Debt as Debt, Debts.Overpayment + Debts.Debt - Debts.Amount as Balance,
	Debts.SalesOrder.Amount + Debts.Amount - Debts.Overpayment - Debts.Debt as Invoiced,
	Debts.SalesOrder.Amount - Debts.Debt as Paid
{select SalesOrder.*, Customer.*, Contract.*, Amount, Debt,
	Balance, Invoiced, Paid}
from Debts as Debts
{where Debts.SalesOrder.Amount as Amount,
	Debts.Debt as Debt, Debts.Overpayment + Debts.Debt - Debts.Amount as Balance,
	Debts.SalesOrder.Amount + Debts.Amount - Debts.Overpayment - Debts.Debt as Invoiced,
	Debts.SalesOrder.Amount - Debts.Debt as Paid}
</query>
