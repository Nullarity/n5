		<dataSource>DataSource1</dataSource>
		<query>//
// Balances
//
select Balances.RowKey as RowKey, Balances.SalesOrder as SalesOrder, Balances.QuantityBalance as Quantity
into Balances
from AccumulationRegister.SalesOrders.Balance ( , SalesOrder in ( &amp;SalesOrders ) ) as Balances
where Balances.QuantityBalance &gt; 0
index by SalesOrder, RowKey
;
//
// Main selection
//
select Items.Ref as SalesOrder, isnull ( Balances.Quantity, 0 ) as Quantity,
	Items.Item as Item, Items.Feature as Feature,
	case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end as DeliveryDate,
	Items.Quantity as Ordered, Items.Price as Price, Items.Amount as ItemsAmount,
	Items.Quantity - isnull ( Balances.Quantity, 0 ) as Shipped
{select SalesOrder.*, Ordered, Item.*, Feature.*, DeliveryDate, Quantity, Price, ItemsAmount, Shipped}
from Document.SalesOrder.Items as Items
	//
	// Balances
	//
	left join Balances as Balances
	on Balances.SalesOrder = Items.Ref
	and Balances.RowKey = Items.RowKey
where Items.Ref in ( &amp;SalesOrders )
{where Items.Item.* as Item, Items.Feature.* as Feature, Items.DocumentOrder.* as DocumentOrder,
case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end as DeliveryDate}
union all
select Services.Ref, isnull ( Balances.Quantity, 0 ), Services.Item, Services.Feature,
	case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end,
	Services.Quantity, Services.Price, Services.Amount, Services.Quantity - isnull ( Balances.Quantity, 0 )
from Document.SalesOrder.Services as Services
	//
	// Balances
	//
	left join Balances as Balances
	on Balances.SalesOrder = Services.Ref
	and Balances.RowKey = Services.RowKey
where Services.Ref in ( &amp;SalesOrders )
{where Services.Item.* as Item, Services.Feature.* as Feature,
case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end as DeliveryDate}
</query>
