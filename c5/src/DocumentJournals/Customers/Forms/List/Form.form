    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Customers.Ref, Customers.Date, Customers.DeletionMark, Customers.Number, Customers.Posted, Customers.Amount,
	Customers.Company, Customers.Creator, Customers.Currency, Customers.Customer, Customers.Duration, Customers.Memo,
	Customers.Subject, Customers.Type, Forms.Number as TaxInvoice, Forms.Form as Form,
	Forms.Status is null as NoStatus,
	case Forms.Status
		when value ( Enum.FormStatuses.Saved ) then 1
		when value ( Enum.FormStatuses.Printed ) then 2
		when value ( Enum.FormStatuses.Canceled ) then 3
		when value ( Enum.FormStatuses.Submitted ) then 4
		when value ( Enum.FormStatuses.Waiting ) then 5
		when value ( Enum.FormStatuses.Unloaded ) then 6
		when value ( Enum.FormStatuses.Returned ) then 6
		else 0
	end as Status
from DocumentJournal.Customers AS Customers
	//
	// Forms
	//
	left join InformationRegister.Forms as Forms
	on Forms.Document = Customers.Ref
</queryText>
