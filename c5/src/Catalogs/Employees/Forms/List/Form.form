    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Hired</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Period</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Status</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Employees.Ref, Employees.DeletionMark, Employees.Parent, Employees.IsFolder, Employees.Code, Employees.Description,
	Employees.BusinessPhone, Employees.Contractor, Employees.Currency, Employees.Email, Employees.EmployeeType,
	Employees.Fax, Employees.FirstName, Employees.HomePhone, Employees.HourlyCost, Employees.HourlyRate, Employees.Individual,
	Employees.LastName, Employees.Memo, Employees.MobilePhone, Employees.Patronymic, Employees.Gender, Employees.Web,
	Employees.Predefined, Employees.PredefinedDataName, Personnel.Department, 
	case when Employees.IsFolder then &amp;Nothing
		when EmployeeStatus.Hired is null then &amp;NotHired 
		when EmployeeStatus.Hired then &amp;HiredFrom
		else &amp;FiredFrom
	end	as Hired,
	EmployeeStatus.Period as Period, cast ( &quot;&quot; as String ( 30 ) ) as Status, Employments.Employment as Employment,
	TasksList.Progress as TaskProgress, TasksList.Completed as TaskCompleted, TasksList.Status as TaskStatus, TasksList.Task as TaskRef, TasksList.Observation as Observation
from Catalog.Employees as Employees
	//
	// Personnel
	//
	{left join InformationRegister.Personnel.SliceLast as Personnel
	on Personnel.Employee = Employees.Ref}
	//
	// Employees
	//
	{left join InformationRegister.Employees.SliceLast ( &amp;CurrentDate ) as EmployeeStatus
	on EmployeeStatus.Employee = Employees.Ref}
	//
	// Employment
	//
	{left join InformationRegister.Employment.SliceLast as Employments
	on Employments.Employee = Employees.Ref}
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as TasksList
	on TasksList.Source = Employees.Ref
	and TasksList.User = &amp;Performer}
{where Personnel.Department as Department, 
	case when EmployeeStatus.Hired is null then &amp;NotHired 
		when EmployeeStatus.Hired then &amp;HiredFrom else
			&amp;FiredFrom
	end	as Hired }
</queryText>
