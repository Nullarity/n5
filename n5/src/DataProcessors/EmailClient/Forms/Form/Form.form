  <attributes>
    <name>Outgoing</name>
    <id>3</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Outgoing.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Outgoing.Status</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Outgoing.Task</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Outgoing.Ref, Outgoing.DeletionMark, Outgoing.Number, Outgoing.Date, Outgoing.Posted, Outgoing.Content, Outgoing.Brief as Brief,
	Outgoing.Mailbox, Outgoing.Subject, Outgoing.IncomingEmail as IncomingEmail,
	cast ( Outgoing.Receiver as string ( 150 ) ) as Receiver,
	case when Outgoing.AttachmentsCount = 0 then 0 else 1 end AttachmentPicture,
	case
		when EmailsAndTasks.Task is not null then
			case when EmailsAndTasks.Task.Executed then 4
				when EmailsAndTasks.Task.Task then 5
				else 6
			end
		when not Outgoing.Posted then 0
		when Replies.Action = &amp;Reply then 3
		when Replies.Action = &amp;Forward then 2
		else 1
	end as Picture,
	case when Statuses.Status is null and Outgoing.Posted then value ( Enum.EmailStatuses.Error ) else Statuses.Status end as Status,
	EmailsAndTasks.Task as Task, EmailsAndTasks.Task.Memo as Memo,
	case when EmailsAndTasks.Task is null then 0
		when EmailsAndTasks.Task.Executed then 1
		when EmailsAndTasks.Task.Task then 2
		else 3
	end as TaskPicture,
	case when EmailsAndTasks.Task.Memo is null then &quot;&quot; else cast ( EmailsAndTasks.Task.Memo as string ( 100 ) ) + &quot;: &quot; end
			+ &amp;To + cast ( Outgoing.Receiver as string ( 100 ) ) + &quot;, &quot;
			+ Outgoing.Subject as SubjectAndEmail
from Document.OutgoingEmail as Outgoing
	//
	// Statuses
	//
	left join InformationRegister.EmailStatuses as Statuses
	on Statuses.Email = Outgoing.Ref
	//
	// Replies
	//
	left join InformationRegister.Replies as Replies
	on Replies.IncomingEmail = Outgoing.IncomingEmail
	and Replies.OutgoingEmail = Outgoing.Ref
	//
	// EmailsAndTasks
	//
	left join InformationRegister.EmailsAndTasks as EmailsAndTasks
	on EmailsAndTasks.Email = Outgoing.Ref
{where EmailsAndTasks.Email &lt;> &amp;EmptyTask, EmailsAndTasks.Task.Executed = &amp;ExecutedTasks}
where not Outgoing.DeletionMark</queryText>
