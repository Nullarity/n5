  <attributes>
    <name>Incoming</name>
    <id>2</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Incoming.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Incoming.Task</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Incoming.Unread</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select allowed Mail.Ref as Ref, Mail.DeletionMark as DeletionMark, Mail.Number as Number, Mail.Email as Email,
	Mail.Date as Date, Mail.Posted as Posted, Mail.Mailbox as Mailbox,
	cast ( Mail.Sender as string ( 150 ) ) as Sender,
	cast ( Mail.Receiver as string ( 150 ) ) as Receiver,
	Mail.Subject as Subject, Mail.Content as Content, Mail.Importance as Importance, Mail.Brief as Brief,
	case when Mail.AttachmentsCount = 0 then 0 else 1 end as AttachmentPicture,
	case when NewMail.IncomingEmail is null then false else true end as Unread,
	case
		when EmailsAndTasks.Task is not null then
			case when EmailsAndTasks.Task.Executed then 5
				when EmailsAndTasks.Task.Task then 6
				else 7
			end
		when NewMail.IncomingEmail is not null then 0
		when &amp;DeletionMark then 2
		when Replies.Action = &amp;Reply then 4
		when Replies.Action = &amp;Forward then 3
		else 1
	end as Picture, EmailsAndTasks.Task as Task, EmailsAndTasks.Task.Memo as Memo,
	case when EmailsAndTasks.Task is null then 0
		when EmailsAndTasks.Task.Executed then 1
		when EmailsAndTasks.Task.Task then 2
		else 3
	end as TaskPicture,
	Replies.OutgoingEmail as LastOutgoingEmail,
	case when Mail.Ref refs Document.OutgoingEmail then &amp;Out else &quot;&quot; end
		+ case when EmailsAndTasks.Task.Memo is null then &quot;&quot; else cast ( EmailsAndTasks.Task.Memo as string ( 100 ) ) + &quot;: &quot; end
		+ Mail.Subject + &quot;, &quot;
		+ cast ( Mail.Email as string ( 100 ) ) as SubjectAndEmail
from DocumentJournal.Mail as Mail
	//
	// Replies
	//
	left join InformationRegister.Replies as Replies
	on Replies.IncomingEmail = Mail.Ref
	//
	// NewMail
	//
	left join InformationRegister.NewMail as NewMail
	on NewMail.User = &amp;User
	and NewMail.IncomingEmail = Mail.Ref
	//
	// EmailsAndTasks
	//
	left join InformationRegister.EmailsAndTasks as EmailsAndTasks
	on EmailsAndTasks.Email = Mail.Ref
{where Mail.Type = &amp;IncomingType, EmailsAndTasks.Email &lt;> &amp;EmptyTask, EmailsAndTasks.Task.Executed = &amp;ExecutedTasks}
where Mail.DeletionMark = &amp;DeletionMark
</queryText>
      <mainTable>DocumentJournal.Mail</mainTable>
