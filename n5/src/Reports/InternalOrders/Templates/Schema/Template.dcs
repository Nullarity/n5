		<dataSource>DataSource1</dataSource>
		<query>//
// Vendors &amp; Items
//
select distinct &amp;Vendor as Vendor, Items.Item as Item
into VendorItems
from InformationRegister.VendorItems as Items
index by Vendor, Item
{where Items.Vendor = &amp;Vendor}
;
//
// Department &amp; Items
//
select distinct &amp;Department as Department, Items.Item as Item
into DepartmentItems
from InformationRegister.DepartmentItems as Items
index by Department, Item
{where Items.Department = &amp;Department}
;
//
// Balances
//
select Balances.RowKey as RowKey, Balances.InternalOrder as InternalOrder, Balances.InternalOrder.Warehouse as Warehouse,
	Balances.QuantityBalance as Quantity
into Balances
from AccumulationRegister.InternalOrders.Balance ( {&amp;Asof}, {InternalOrder.* as InternalOrder} ) as Balances
{where Balances.InternalOrder.Warehouse.* as Warehouse}
index by InternalOrder, RowKey
;
//
// Items and Services
//
select Items.Ref as InternalOrder, Items.RowKey as RowKey, Items.Item as Item, false as Service,Items.Package as Package,
	isnull ( Items.Package.Capacity, 1 ) as Capacity, "" as Description,
	case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end as DeliveryDate,
	Items.Feature as Feature, Items.QuantityPkg as QuantityPkgOrdered, Items.Quantity as QuantityOrdered,
	Balances.Warehouse as Warehouse, Balances.Quantity as Quantity, Items.Price as Price, Items.Amount as Amount,
	Items.Prices as Prices, Items.VATCode as VATCode, Items.VATRate as VATRate
into Items
from Document.InternalOrder.Items as Items
	//
	// Balances
	//
	join Balances as Balances
	on Balances.InternalOrder = Items.Ref
	and Balances.RowKey = Items.RowKey
	//
	// Filter by VendorItems
	//
	{join VendorItems as VendorItems
	on VendorItems.Item = Items.Item}
	//
	// Filter by DepartmentItems
	//
	{join DepartmentItems as DepartmentItems
	on DepartmentItems.Item = Items.Item}
{where Items.Item.* as Item, false as Service, Items.Package.* as Package, Items.Feature.* as Feature,
case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end between &amp;DeliveryDateStart and &amp;DeliveryDateEnd, 
VendorItems.Vendor = &amp;Vendor, DepartmentItems.Department = &amp;Department}
union all
select Services.Ref, Services.RowKey, Services.Item, true, null, 1, Description,
	case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end,
	Services.Feature, Services.Quantity, Services.Quantity, Balances.Warehouse, Balances.Quantity,
	Services.Price, Services.Amount, Services.Prices, Services.VATCode, Services.VATRate
from Document.InternalOrder.Services as Services
	//
	// Balances
	//
	join Balances as Balances
	on Balances.InternalOrder = Services.Ref
	and Balances.RowKey = Services.RowKey
	//
	// Filter by VendorItems
	//
	{join VendorItems as VendorItems
	on VendorItems.Item = Services.Item}
	//
	// Filter by DepartmentItems
	//
	{join DepartmentItems as DepartmentItems
	on DepartmentItems.Item = Services.Item}
{where Services.Item.* as Item, true as Service, null as Package, Services.Feature.* as Feature,
case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end between &amp;DeliveryDateStart and &amp;DeliveryDateEnd, 
VendorItems.Vendor = &amp;Vendor, DepartmentItems.Department = &amp;Department}
index by Warehouse, InternalOrder, RowKey, Item, Package, Feature
;
//
// PurchaseOrders
//
select PurchaseOrders.Ref as PurchaseOrder, PurchaseOrders.DocumentOrder as InternalOrder, PurchaseOrders.RowKey as PurchaseOrderRowKey, PurchaseOrders.DocumentOrderRowKey as InternalOrderRowKey
into PurchaseOrders
from Document.PurchaseOrder.Items as PurchaseOrders
	//
	// Items
	//
	join Items as Items
	on PurchaseOrders.DocumentOrder = Items.InternalOrder
	and PurchaseOrders.DocumentOrderRowKey = Items.RowKey
index by PurchaseOrder, PurchaseOrderRowKey, DocumentOrder, DocumentOrderRowKey
;
//
// ProductionOrders
//
select ProductionOrders.Ref as ProductionOrder, ProductionOrders.DocumentOrder as InternalOrder, ProductionOrders.RowKey as ProductionOrderRowKey, ProductionOrders.DocumentOrderRowKey as InternalOrderRowKey
into ProductionOrders
from Document.ProductionOrder.Items as ProductionOrders
	//
	// Items
	//
	join Items as Items
	on ProductionOrders.DocumentOrder = Items.InternalOrder
	and ProductionOrders.DocumentOrderRowKey = Items.RowKey
index by ProductionOrder, ProductionOrderRowKey, DocumentOrder, DocumentOrderRowKey
;
select PurchaseOrders.InternalOrder as InternalOrder, PurchaseOrders.InternalOrderRowKey as InternalOrderRowKey, PurchaseOrdersBalance.QuantityBalance as QuantityAllocated
into Allocated
from AccumulationRegister.PurchaseOrders.Balance ( {&amp;Asof}, ( PurchaseOrder, RowKey ) in ( select distinct PurchaseOrder, PurchaseOrderRowKey from PurchaseOrders ) ) as PurchaseOrdersBalance
	//
	// PurchaseOrders
	//
	join PurchaseOrders as PurchaseOrders
	on PurchaseOrders.PurchaseOrder = PurchaseOrdersBalance.PurchaseOrder
	and PurchaseOrders.PurchaseOrderRowKey = PurchaseOrdersBalance.RowKey
union
select ProductionOrders.InternalOrder, ProductionOrders.InternalOrderRowKey, ProductionOrdersBalance.QuantityBalance
from AccumulationRegister.ProductionOrders.Balance ( {&amp;Asof}, ( ProductionOrder, RowKey ) in ( select distinct ProductionOrder, ProductionOrderRowKey from ProductionOrders ) ) as ProductionOrdersBalance
	//
	// ProductionOrders
	//
	join ProductionOrders as ProductionOrders
	on ProductionOrders.ProductionOrder = ProductionOrdersBalance.ProductionOrder
	and ProductionOrders.ProductionOrderRowKey = ProductionOrdersBalance.RowKey	
index by InternalOrder, InternalOrderRowKey
;
//
// Main selection
//
select allowed Items.Item as Item, Items.Service as Service, Items.Package as Package, Items.Capacity as Capacity, Items.Feature as Feature,
	Items.RowKey as RowKey, Items.InternalOrder as InternalOrder, Items.Warehouse as Warehouse, Items.DeliveryDate as DeliveryDate,
	Items.QuantityPkgOrdered as QuantityPkgOrdered, Items.Quantity as Quantity, Items.Price as Price, Items.Description as Description,
	case when Items.QuantityOrdered = Items.Quantity then Items.Amount else Items.Quantity * Items.Price end as Amount,
	Items.QuantityOrdered as QuantityOrdered, Items.Prices as Prices, Items.Quantity / Items.Capacity as QuantityPkgBalance,
	( Items.QuantityOrdered - Items.Quantity ) as QuantityGot,
	( Items.QuantityPkgOrdered - ( Items.Quantity / Items.Capacity ) ) as QuantityPkgGot,
	Reserves.QuantityBalance / Items.Capacity as QuantityPkgReserve,
	Reserves.QuantityBalance as QuantityReserve,
	Allocation.QuantityBalance / Items.Capacity as QuantityPkgForAllocation,
	Allocation.QuantityBalance as QuantityForAllocation,
	Allocated.QuantityAllocated / Items.Capacity as QuantityPkgAllocated,
	Allocated.QuantityAllocated as QuantityAllocated, Items.VATCode as VATCode, Items.VATRate as VATRate
{select InternalOrder.*, Warehouse.*, Item.*, Service, Package.*, Capacity, Feature.*,
	Quantity, QuantityOrdered, QuantityPkgOrdered, QuantityGot, QuantityPkgGot, QuantityPkgBalance,
	Price, Amount, QuantityReserve, QuantityPkgReserve, QuantityForAllocation, QuantityPkgForAllocation,
	QuantityAllocated, QuantityPkgAllocated, DeliveryDate, RowKey, Prices.*, VATCode.*, VATRate, Description}
from Items as Items
	//
	// Reserves
	//
	{left join AccumulationRegister.Reserves.Balance ( {&amp;Asof}, ( DocumentOrder, RowKey ) in ( select distinct InternalOrder, RowKey from Items ) ) as Reserves
	on Reserves.DocumentOrder = Items.InternalOrder
	and Reserves.RowKey = Items.RowKey}
	//
	// Allocation
	//
	{left join AccumulationRegister.Allocation.Balance ( {&amp;Asof}, ( DocumentOrder, RowKey ) in ( select distinct InternalOrder, RowKey from Items ) ) as Allocation
	on Allocation.DocumentOrder = Items.InternalOrder
	and Allocation.RowKey = Items.RowKey}
	//
	// Allocated
	//
	{left join Allocated as Allocated
	on Allocated.InternalOrder = Items.InternalOrder
	and Allocated.InternalOrderRowKey = Items.RowKey}
{where Items.QuantityPkgOrdered as QuantityPkgOrdered,
Items.Quantity / Items.Capacity as QuantityPkgBalance,
( Items.QuantityPkgOrdered - ( Items.Quantity / Items.Capacity ) ) as QuantityPkgGot,
Reserves.QuantityBalance / Items.Capacity as QuantityPkgReserve,
Allocation.QuantityBalance / Items.Capacity as QuantityPkgForAllocation,
Allocated.QuantityAllocated / Items.Capacity as QuantityPkgAllocated}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
