		<query>// Payroll
select Compensations.Individual as Employee, Compensations.Compensation as Compensation, Compensations.Days as Days,
	Compensations.AccountingResult as Calculated, Compensations.Employee as PayrollEmployee, Compensations.Hours as Hours,
	Compensations.ScheduledDays as ScheduledDays, Compensations.ScheduledHours as ScheduledHours
{select Employee.*, PayrollEmployee.* as PayrollEmployee, Compensation.*, Calculated, PayrollEmployee, Days, Hours, ScheduledDays, ScheduledHours}
from Document.Payroll.Compensations as Compensations
{where Compensations.Department = &amp;Department, Compensations.Ref.Company = &amp;Company, Compensations.Individual = &amp;Employee}
where Compensations.Ref.Date between &amp;DateStart and &amp;DateEnd
and Compensations.Ref.Posted
</query>
