		<dataSource>DataSource1</dataSource>
		<query>// Employees
select distinct Compensations.Individual as Employee, Compensations.Employee.Code as Code, Compensations.Position as Position,
	Compensations.Employee as PayrollEmployee
{select Employee.*, Code, Position.*, PayrollEmployee}
from Document.Payroll.Compensations as Compensations
{where Compensations.Department = &amp;Department, Compensations.Ref.Company = &amp;Company, Compensations.Individual = &amp;Employee}
where Compensations.Ref.Date between &amp;DateStart and &amp;DateEnd
and Compensations.Ref.Posted
union all
select distinct Totals.Employee, Personnel.Employee.Code, Personnel.Position, Personnel.Employee
from Document.PayEmployees.Totals as Totals
	//
	//	Personnel
	//
	{left join InformationRegister.Personnel.SliceLast ( &amp;DateEnd ) as Personnel
	on Personnel.Employee.Individual = Totals.Employee}
{where Personnel.Department = &amp;Department, Totals.Ref.Company = &amp;Company, Totals.Employee = &amp;Employee}
where Totals.Ref.Date between &amp;DateStart and &amp;DateEnd
and Totals.Ref.Posted
union all
select distinct Totals.Employee, Personnel.Employee.Code, Personnel.Position, Personnel.Employee
from Document.PayAdvances.Totals as Totals
	//
	//	Personnel
	//
	{left join InformationRegister.Personnel.SliceLast ( &amp;DateEnd ) as Personnel
	on Personnel.Employee.Individual = Totals.Employee}
{where Personnel.Department = &amp;Department, Totals.Ref.Company = &amp;Company, Totals.Employee = &amp;Employee}
where Totals.Ref.Date between &amp;DateStart and &amp;DateEnd
and Totals.Ref.Posted</query>
