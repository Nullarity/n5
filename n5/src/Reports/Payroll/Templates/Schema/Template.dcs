		<dataSource>DataSource1</dataSource>
		<query>// Pay employees totals
select Totals.Employee as Employee, Totals.Amount as Brutto,
	case when Totals.Ref.Method = value ( Enum.PaymentMethods.EmptyRef ) then 0 else Totals.Net end as Net,
	0 as Advance
{select Brutto, Net, Advance, Employee.*}
from Document.PayEmployees.Totals as Totals
{where Totals.Ref.Company = &amp;Company, Totals.Employee = &amp;Employee}
where Totals.Ref.Date between &amp;DateStart and &amp;DateEnd
and Totals.Ref.Posted
union all
select Totals.Employee, Totals.Amount,
	case when Totals.Ref.Method = value ( Enum.PaymentMethods.EmptyRef ) then 0 else Totals.Net end,
	case when Totals.Ref.Method = value ( Enum.PaymentMethods.EmptyRef ) then 0 else Totals.Net end
from Document.PayAdvances.Totals as Totals
{where Totals.Ref.Company = &amp;Company, Totals.Employee = &amp;Employee}
where Totals.Ref.Date between &amp;DateStart and &amp;DateEnd
and Totals.Ref.Posted	</query>
		<autoFillFields>false</autoFillFields>
