		<query>// Balances Deposits
select Balances.ExtDimension1 as Employee, Balances.AmountOpeningBalanceCr as DepositBegin, 
	AmountClosingBalanceCr as DepositEnd, Balances.AmountTurnoverDr as Deposited
{select	Employee.*, DepositBegin, DepositEnd, Deposited}	
from AccountingRegister.General.BalanceAndTurnovers ( &amp;DateStart, &amp;DateEnd, , , Account in ( 
		select Settings.Value as Account
		from InformationRegister.Settings as Settings
		where Settings.Parameter = value ( ChartOfCharacteristicTypes.Settings.DepositLiabilities )
			and Settings.Period &lt;= &amp;DateEnd
		group by Settings.Value ), , {Company = &amp;Company, ExtDimension1 = &amp;Employee}) as Balances
</query>
