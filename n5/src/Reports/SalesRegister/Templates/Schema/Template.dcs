	<dataSet xsi:type="DataSetQuery">
		<name>Signatures</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Accountant</dataPath>
			<field>Accountant</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Director</dataPath>
			<field>Director</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>select allowed Accountant.Name as Accountant, Director.Name as Director
{select Accountant, Director}
from Catalog.Companies as Company
	//
	// Accountant
	//
	left join (
		select top 1 Roles.User.Employee.Individual.Description as Name
		from Document.Roles as Roles
		where Roles.Role = value ( Enum.Roles.AccountantChief )
		and not Roles.DeletionMark
		and Roles.Action = value ( Enum.AssignRoles.Assign )
		and Roles.Company = &amp;Company
		order by Roles.Date desc
			) as Accountant
	on true
	//
	// Director
	//
	left join (
		select top 1 Roles.User.Employee.Individual.Description as Name
		from Document.Roles as Roles
		where Roles.Role = value ( Enum.Roles.GeneralManager )
		and not Roles.DeletionMark
		and Roles.Action = value ( Enum.AssignRoles.Assign )
		and Roles.Company = &amp;Company
		order by Roles.Date desc
			) as Director
	on true
where Company.Ref = &amp;Company</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
	<dataSet xsi:type="DataSetQuery">
		<name>Descriptions</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>ItemType</dataPath>
			<field>ItemType</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>DescriptionsRef</dataPath>
			<field>DescriptionsRef</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Export</dataPath>
			<field>Export</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Export</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>select allowed Table.Ref as DescriptionsRef,
	case valuetype ( Table.Item )
		when type ( Catalog.Items ) then cast ( Table.Item  as Catalog.Items ).ItemType.Description
		when type ( Catalog.FixedAssets ) then
			cast ( cast ( Table.Item as Catalog.FixedAssets ).AssetType.Name as String ( 250 ) )
		when type ( Catalog.IntangibleAssets ) then
			cast ( cast ( Table.Item as Catalog.IntangibleAssets ).AssetType.Name as String ( 250 ) )
	end as ItemType
from Document.InvoiceRecord.Items as Table
where Table.VATCode.Type &lt;&gt; value ( Enum.VAT.None )
and Table.Ref in ( &amp;Documents )
{where not &amp;Export}
union
select Table.Ref, Table.Item.ItemType.Description
from Document.InvoiceRecord.Services as Table
where Table.VATCode.Type &lt;&gt; value ( Enum.VAT.None )
and Table.Ref in ( &amp;Documents )
{where not &amp;Export}
union
select Table.Ref, Table.Item.ItemType.Description
from Document.RetailSales.Items as Table
where Table.VATCode.Type &lt;&gt; value ( Enum.VAT.None )
and Table.Ref in ( &amp;Documents )
{where not &amp;Export}
union
select Table.Ref, Table.Item.ItemType.Description
from Document.Invoice.Items as Table
where Table.VATCode.Type &lt;&gt; value ( Enum.VAT.None )
and Table.Ref in ( &amp;Documents )
and Table.Ref.Contract.Export
{where &amp;Export}
union
select Table.Ref, Table.Item.ItemType.Description
from Document.Invoice.Services as Table
where Table.VATCode.Type &lt;&gt; value ( Enum.VAT.None )
and Table.Ref in ( &amp;Documents )
and Table.Ref.Contract.Export
{where &amp;Export}
</query>
