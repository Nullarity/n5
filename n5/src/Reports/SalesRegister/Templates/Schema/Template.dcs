		<dataSource>DataSource1</dataSource>
		<query>select Advances.Recorder as Recorder, sum ( Advances.Amount ) as Amount, sum ( Advances.VAT ) as VAT,
	Advances.VATCode as VATCode, Advances.Customer as Customer
into Advances
from (
	select General.Recorder as Recorder,
		case
			when General.Operation in (
				value ( Enum.Operations.AdvanceTaken ),
				value ( Enum.Operations.AdvanceTakenAfterAdjustments ) ) then General.Amount
			when General.Operation = value ( Enum.Operations.AdvanceApplied ) then - General.Amount
			else 0
		end as Amount,
		case when General.Operation in (
			value ( Enum.Operations.VATAdvances ),
			value ( Enum.Operations.VATAdvancesReverse ) ) then General.Amount
			else 0
		end as VAT,
		case
			when valuetype ( General.Recorder ) = type ( Document.Payment ) then cast ( General.Recorder as Document.Payment ).VATAdvance
			when valuetype ( General.Recorder ) = type ( Document.AdjustDebts ) then cast ( General.Recorder as Document.AdjustDebts ).VATAdvance
			when valuetype ( General.Recorder ) = type ( Document.AdjustVendorDebts ) then cast ( General.Recorder as Document.AdjustVendorDebts ).VATAdvance
			when valuetype ( General.Dependency ) = type ( Document.Payment ) then cast ( General.Dependency as Document.Payment ).VATAdvance
			when valuetype ( General.Dependency ) = type ( Document.AdjustDebts ) then cast ( General.Dependency as Document.AdjustDebts ).VATAdvance
			when valuetype ( General.Dependency ) = type ( Document.AdjustVendorDebts ) then cast ( General.Dependency as Document.AdjustVendorDebts ).VATAdvance
			when valuetype ( General.Dependency ) = type ( Document.Debts ) then cast ( General.Dependency as Document.Debts ).VATAdvance
		end as VATCode,
		case
			when General.Operation in (
				value ( Enum.Operations.AdvanceApplied ),
				value ( Enum.Operations.VATAdvances ),
				value ( Enum.Operations.VATAdvancesReverse ) ) then General.ExtDimensionDr1
			else General.ExtDimensionCr1
		end as Customer
	from AccountingRegister.General.RecordsWithExtDimensions ( &amp;DateStart, &amp;DateEnd, Company = &amp;Company ) as General
	where General.Operation in (
		value ( Enum.Operations.AdvanceApplied ),
		value ( Enum.Operations.AdvanceTaken ),
		value ( Enum.Operations.AdvanceTakenAfterAdjustments ),
		value ( Enum.Operations.VATAdvances ),
		value ( Enum.Operations.VATAdvancesReverse )
	)
) as Advances
group by Advances.Recorder, Advances.VATCode, Advances.Customer
index by Recorder
;
// Main selection
select Sales.VAT as VAT, Sales.Amount as Amount, Sales.Ref as Ref,
	Sales.VAT + Sales.Amount as Total, Sales.Rate as Rate, Sales.Date as Date, 
	Sales.Number as Number, Sales.CodeFiscal as CodeFiscal, Sales.Customer as Customer,
	Sales.Description as Description, Sales.Base as Base
{select VAT, Amount, Total, Rate.*, CodeFiscal, Customer.*, Number, Date, Ref.*, Description, Base}
from (
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end as VAT, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end as Amount, 
		Table.VATCode as Rate, Table.Ref.Number as Number, Table.Ref.DeliveryDate as Date, 
		Table.Ref.Customer.CodeFiscal as CodeFiscal, Table.Ref.Customer as Customer, Table.Ref as Ref, Table.Ref.Description as Description,
		Table.Ref.Base as Base
	from Document.InvoiceRecord.Items as Table
	where not Table.Ref.DeletionMark
	and not Table.Ref.Transfer
	and not Table.Ref.Base refs Document.VendorReturn
	and not Table.Ref.Base refs Document.WriteOff
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Ref.Status in (
		value ( Enum.FormStatuses.Unloaded ),
		value ( Enum.FormStatuses.Printed ),
		value ( Enum.FormStatuses.Submitted ),
		value ( Enum.FormStatuses.Returned )
	)
	{where not &amp;Export}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Number, Table.Ref.DeliveryDate, 
		Table.Ref.Customer.CodeFiscal, Table.Ref.Customer, Table.Ref, Table.Ref.Description,
		Table.Ref.Base
	from Document.InvoiceRecord.Services as Table
	where not Table.Ref.DeletionMark
	and not Table.Ref.Transfer
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Ref.Status in (
		value ( Enum.FormStatuses.Unloaded ),
		value ( Enum.FormStatuses.Printed ),
		value ( Enum.FormStatuses.Submitted ),
		value ( Enum.FormStatuses.Returned )
	)
	{where not &amp;Export}
	union all
	select - case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		- case when Table.Ref.Currency = &amp;Currency then Table.Amount - Table.VAT else cast ( ( Table.Amount - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Number, Table.Ref.DeliveryDate, 
		Table.Ref.Customer.CodeFiscal, Table.Ref.Customer, Table.Ref, Table.Ref.Description,
		Table.Ref.Base
	from Document.InvoiceRecord.Discounts as Table
	where not Table.Ref.DeletionMark
	and not Table.Ref.Transfer
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Ref.Status in (
		value ( Enum.FormStatuses.Unloaded ),
		value ( Enum.FormStatuses.Printed ),
		value ( Enum.FormStatuses.Submitted ),
		value ( Enum.FormStatuses.Returned )
	)
	{where not &amp;Export}
	union all
	select Table.VAT, Table.Total - Table.VAT,	Table.VATCode, Table.Number, Table.Date, 
		Table.Customer.CodeFiscal, Table.Customer, Table.Ref, Table.Description, Table.Base
	from Document.VATSales as Table
	where not Table.Ref.DeletionMark
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	{where not &amp;Export}
	union all
	select Advances.VAT, Advances.Amount - Advances.VAT, Advances.VATCode,
		CustomerPayments.Number, CustomerPayments.Date, Advances.Customer.CodeFiscal,
		Advances.Customer, CustomerPayments.Ref, value ( Enum.Operations.VATAdvances ), undefined
	from Document.Payment as CustomerPayments
		//
		// CustomerPayments Advances
		//
		join Advances as Advances
		on Advances.Recorder = CustomerPayments.Ref
	{where not &amp;Export}
	union all
	select Advances.VAT, Advances.Amount - Advances.VAT, Advances.VATCode,
		Refunds.Number, Refunds.Date, Advances.Customer.CodeFiscal,
		Advances.Customer, Refunds.Ref, value ( Enum.Operations.VATAdvancesReverse ), undefined
	from Document.Refund as Refunds
		//
		// Refunds
		//
		join Advances as Advances
		on Advances.Recorder = Refunds.Ref
	{where not &amp;Export}
	union all
	select Advances.VAT, Advances.Amount - Advances.VAT, Advances.VATCode,
		Invoices.Number, Invoices.Date, Advances.Customer.CodeFiscal,
		Advances.Customer, Invoices.Ref, value ( Enum.Operations.VATAdvancesReverse ), undefined
	from Document.Invoice as Invoices
		//
		// Invoices Advances
		//
		join Advances as Advances
		on Advances.Recorder = Invoices.Ref
	{where not &amp;Export}
	union all
	select Advances.VAT, Advances.Amount - Advances.VAT, Advances.VATCode,
		AdjustDebts.Number, AdjustDebts.Date, Advances.Customer.CodeFiscal,
		Advances.Customer, AdjustDebts.Ref, value ( Enum.Operations.VATAdvancesReverse ), undefined
	from Document.AdjustDebts as AdjustDebts
		//
		// AdjustDebts Advances
		//
		join Advances as Advances
		on Advances.Recorder = AdjustDebts.Ref
	{where not &amp;Export}
	union all
	select Advances.VAT, Advances.Amount - Advances.VAT, Advances.VATCode,
		AdjustVendorDebts.Number, AdjustVendorDebts.Date, Advances.Customer.CodeFiscal,
		Advances.Customer, AdjustVendorDebts.Ref, value ( Enum.Operations.VATAdvancesReverse ), undefined
	from Document.AdjustVendorDebts as AdjustVendorDebts
		//
		// AdjustVendorDebts Advances
		//
		join Advances as Advances
		on Advances.Recorder = AdjustVendorDebts.Ref
	{where not &amp;Export}
	union all
	select Advances.VATAmount, case when Advances.CloseAdvance then -1 else 1 end
		* Advances.Amount - Advances.VATAmount,
		Advances.VAT, Advances.Ref.Number, Advances.Ref.Date, Advances.Customer.CodeFiscal,
		Advances.Customer, Advances.Ref,
		case when Advances.CloseAdvance then value ( Enum.Operations.VATAdvancesReverse )
			else value ( Enum.Operations.VATAdvances )
		end,
		undefined
	from Document.ClosingAdvances.Advances as Advances
	where Advances.Ref.Posted
	and Advances.VATAmount &lt;&gt; 0
	and Advances.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Advances.Ref.Company = &amp;Company
	{where not &amp;Export}
	union all
	select sum ( Table.VAT ), sum ( Table.Amount ), Table.Rate, Table.Ref.Number,
		Table.Ref.Date, undefined, Table.Ref.Warehouse.Description + ", " +
			cast ( case Table.Ref.Method
				when value ( Enum.PaymentMethods.Cash ) then &amp;MethodCash
				when value ( Enum.PaymentMethods.Card ) then &amp;MethodCard
				else &amp;MethodOther
			end as string ( 25 ) )
		, Table.Ref, null, undefined
	from (
		select Table.VAT - isnull ( Goods.VAT, 0 ) as VAT,
			Table.Total - Table.VAT - isnull ( Goods.Amount, 0 ) as Amount,
			Table.VATCode as Rate, Table.Ref as Ref
		from Document.RetailSales.Items as Table
			//
			// Goods
			//
			left join (
				select Goods.Ref.Warehouse as Warehouse, Goods.Ref.Location as Location, Goods.Ref.Method as Method,
					Goods.Item as Item, Goods.VATCode as VATCode, Goods.Package as Package,
					Goods.Feature as Feature, Goods.Series as Series,
					sum ( Goods.VAT ) as VAT, sum ( Goods.Total - Goods.VAT ) as Amount,
					beginofperiod ( Goods.Ref.Date, day ) as Day, Goods.Price as Price
				from Document.Sale.Items as Goods
				//
				// Only Invoice Records
				//
				join Document.InvoiceRecord as InvoiceRecords
				on InvoiceRecords.Base = Goods.Ref
				and not InvoiceRecords.DeletionMark
				and InvoiceRecords.Status in (
					value ( Enum.FormStatuses.Unloaded ),
					value ( Enum.FormStatuses.Printed ),
					value ( Enum.FormStatuses.Submitted ),
					value ( Enum.FormStatuses.Returned )
				)
				where Goods.Ref.Posted
				and Goods.Ref.Date between &amp;DateStart and &amp;DateEnd
				and Goods.Ref.Company = &amp;Company
				and Goods.Ref.VATUse &lt;&gt; 0
				group by beginofperiod ( Goods.Ref.Date, day ), Goods.Ref.Location, Goods.Ref.Warehouse,
				Goods.Ref.Method, Goods.VATCode, Goods.Item, Goods.Package,
				Goods.Feature, Goods.Series, Goods.Price
			) as Goods
			on Goods.Location = Table.Ref.Location
			and Goods.Warehouse = Table.Ref.Warehouse
			and Goods.Method = Table.Ref.Method
			and Goods.Item = Table.Item
			and Goods.VATCode = Table.VATCode
			and Goods.Package = Table.Package
			and Goods.Feature = Table.Feature
			and Goods.Series = Table.Series
			and Goods.Price = Table.Price
			and Goods.Day = beginofperiod ( Table.Ref.Date, day )
		where Table.Ref.Posted
		and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
		and Table.Ref.Company = &amp;Company
		and Table.Ref.VATUse &lt;&gt; 0
		and ( Table.VAT - isnull ( Goods.VAT, 0 ) ) &lt;&gt; 0
	) as Table
	group by Table.Ref, Table.Rate
	{where not &amp;Export}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Number, Table.Ref.Date, 
		Table.Ref.Customer.CodeFiscal, Table.Ref.Customer, Table.Ref, "",
		Table.Ref
	from Document.Invoice.Items as Table
	where Table.Ref.Posted
	and Table.Ref.Contract.Export
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	{where &amp;Export}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Number, Table.Ref.Date, 
		Table.Ref.Customer.CodeFiscal, Table.Ref.Customer, Table.Ref, "",
		Table.Ref
	from Document.Invoice.Services as Table
	where Table.Ref.Posted
	and Table.Ref.Contract.Export
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	{where &amp;Export}
) as Sales
</query>
