		<dataSource>DataSource1</dataSource>
		<query>//
// ItemKeys
//
select Details.ItemKey as ItemKey, Details.Item as Item, Details.Feature as Feature,
	Details.Series as Series, Details.Package as Package, Details.Warehouse as Warehouse
into Details
from InformationRegister.ItemDetails as Details
where Details.Item in ( &amp;Items )
and Details.Warehouse in ( &amp;Warehouses )
index by ItemKey
;
//
// Main Selection
//
select Details.Item as Item, Details.Package as Package, Details.Feature as Feature,
	Details.Series as Series, Details.Warehouse as Warehouse, Cost.ItemKey as ItemKey,
	Cost.AmountBalance as CostAmount, Cost.QuantityBalance as CostQuantity,
	Cost.AmountBalance / Cost.QuantityBalance as UnitCost
{select CostAmount, CostQuantity, Item.*, Package.*, Feature.*, Series.*, Warehouse.*, UnitCost, ItemKey}
from AccumulationRegister.Cost.Balance ( , ItemKey in ( select ItemKey from Details ) ) as Cost
	//
	// Details
	//
	join Details as Details
	on Details.ItemKey = Cost.ItemKey
where Cost.QuantityBalance &lt;&gt; 0
</query>
