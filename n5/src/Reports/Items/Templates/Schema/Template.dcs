		<dataSource>DataSource1</dataSource>
		<query>//
// Reserves
//
select allowed Reserves.DocumentOrder as DocumentOrder, Reserves.RowKey as RowKey,
	Reserves.Warehouse as Warehouse, Reserves.SecondPeriod as SecondPeriod,
	case when ( Reserves.Recorder = undefined ) then null else Reserves.Recorder end as Recorder,
	Reserves.QuantityOpeningBalance as QuantityOpeningBalance,
	Reserves.QuantityClosingBalance as QuantityClosingBalance,
	Reserves.QuantityReceipt as QuantityReceipt,
	Reserves.QuantityExpense as QuantityExpense
{select SecondPeriod, Recorder.*, QuantityOpeningBalance, QuantityClosingBalance,
	QuantityReceipt, QuantityExpense}
into Reserves
from AccumulationRegister.Reserves.BalanceAndTurnovers ( {&amp;DateStart}, {&amp;DateEnd}, auto, ,
	{Warehouse.* as Warehouse, Warehouse.Owner.* as Company}
	Warehouse in ( select Ref from Catalog.Warehouses )
	and Warehouse.Owner in ( select Ref from Catalog.Companies ) ) as Reserves
index by DocumentOrder, RowKey
;
//
// Main Selection
//
select allowed Balances.Item as Item, Balances.Feature as Feature, Balances.Series as Series,
	Balances.Package as Package, Balances.Warehouse as Warehouse, Balances.Warehouse.Owner as Company,
	Balances.SecondPeriod as SecondPeriod,
	case when ( Balances.Recorder = undefined ) then null else Balances.Recorder end as Recorder,
	Balances.QuantityClosingBalance as QuantityBaseClosingBalance,
	( Balances.QuantityClosingBalance - Balances.ReserveClosing ) / Balances.Capacity as Available,
	Balances.QuantityOpeningBalance / Balances.Capacity as QuantityOpeningBalance,
	Balances.QuantityClosingBalance / Balances.Capacity as QuantityClosingBalance,
	Balances.QuantityReceipt / Balances.Capacity as QuantityReceipt,
	Balances.QuantityExpense / Balances.Capacity as QuantityExpense,
	Balances.ReserveOpening / Balances.Capacity as ReserveOpening,
	Balances.ReserveClosing / Balances.Capacity as ReserveClosing,
	Balances.ReserveReceipt / Balances.Capacity as ReserveReceipt,
	Balances.ReserveExpense / Balances.Capacity as ReserveExpense
{select Item.*, Feature.*, Series.*, Package.*, Warehouse.*, Company.*, QuantityOpeningBalance,
	QuantityClosingBalance, QuantityExpense, QuantityReceipt, QuantityBaseClosingBalance,
	SecondPeriod, Recorder.*, ReserveOpening, ReserveClosing, ReserveReceipt, ReserveExpense,
	Available}
from (	
	select Balances.Item as Item, Balances.Feature as Feature, Balances.Series as Series,
		Balances.Package as Package, isnull ( Balances.Package.Capacity, 1 ) as Capacity,
		Balances.Warehouse as Warehouse, Balances.SecondPeriod as SecondPeriod,
		case when ( Balances.Recorder = undefined ) then null else Balances.Recorder end as Recorder,
		Balances.QuantityOpeningBalance * isnull ( Balances.Package.Capacity, 1 ) as QuantityOpeningBalance,
		Balances.QuantityClosingBalance * isnull ( Balances.Package.Capacity, 1 ) as QuantityClosingBalance,
		Balances.QuantityReceipt * isnull ( Balances.Package.Capacity, 1 ) as QuantityReceipt,
		Balances.QuantityExpense * isnull ( Balances.Package.Capacity, 1 ) as QuantityExpense,
		0 as ReserveOpening, 0 as ReserveClosing, 0 as ReserveReceipt, 0 as ReserveExpense
	{select Item, Feature, Series, Package, Warehouse, SecondPeriod, Recorder, QuantityOpeningBalance,
		QuantityClosingBalance, QuantityReceipt, QuantityExpense, ReserveOpening, ReserveClosing,
		ReserveReceipt, ReserveExpense}
	from AccumulationRegister.Items.BalanceAndTurnovers ( {&amp;DateStart}, {&amp;DateEnd}, auto, ,
		{Item.* as Item, Feature.* as Feature, Series.* as Series, Warehouse.* as Warehouse, Warehouse.Owner.* as Company}
		Warehouse in ( select Ref from Catalog.Warehouses )
		and Warehouse.Owner in ( select Ref from Catalog.Companies ) ) as Balances
	{where Balances.Item.* as Item, Balances.Feature.* as Feature, Balances.Series.* as Series}
	union all
	select Items.Item, Items.Feature, Items.Series,
		case when Items.Item.CountPackages then Items.Package else value ( Catalog.Packages.EmptyRef ) end,
		case when Items.Item.CountPackages then isnull ( Items.Package.Capacity, 1 ) else 1 end,
		Reserves.Warehouse, Reserves.SecondPeriod,
		case when ( Reserves.Recorder = undefined ) then null else Reserves.Recorder end,
		Reserves.QuantityOpeningBalance, Reserves.QuantityClosingBalance,
		Reserves.QuantityReceipt, Reserves.QuantityExpense,
		Reserves.QuantityOpeningBalance, Reserves.QuantityClosingBalance,
		Reserves.QuantityReceipt, Reserves.QuantityExpense
	from Document.SalesOrder.Items as Items
		//
		// Reserves
		//
		join Reserves as Reserves
		on Reserves.DocumentOrder = Items.Ref
		and Reserves.RowKey = Items.RowKey
	{where Items.Item.* as Item, Items.Feature.* as Feature, Items.Series.* as Series}
	union all
	select Items.Item, Items.Feature, Items.Series,
		case when Items.Item.CountPackages then Items.Package else value ( Catalog.Packages.EmptyRef ) end,
		case when Items.Item.CountPackages then isnull ( Items.Package.Capacity, 1 ) else 1 end,
		Reserves.Warehouse, Reserves.SecondPeriod,
		case when ( Reserves.Recorder = undefined ) then null else Reserves.Recorder end,
		Reserves.QuantityOpeningBalance, Reserves.QuantityClosingBalance,
		Reserves.QuantityReceipt, Reserves.QuantityExpense,
		Reserves.QuantityOpeningBalance, Reserves.QuantityClosingBalance,
		Reserves.QuantityReceipt, Reserves.QuantityExpense
	from Document.InternalOrder.Items as Items
		//
		// Reserves
		//
		join Reserves as Reserves
		on Reserves.DocumentOrder = Items.Ref
		and Reserves.RowKey = Items.RowKey
	{where Items.Item.* as Item, Items.Feature.* as Feature, Items.Series.* as Series}
) as Balances
{where Balances.Package.* as Package,
( Balances.QuantityClosingBalance - ReserveClosing ) / Balances.Capacity as Available}
</query>
