		<dataSource>DataSource1</dataSource>
		<query>//
// Main selection
//
select Work.SecondPeriod as SecondPeriod, case when ( Work.Recorder = undefined ) then null else Work.Recorder end as Recorder,
	Work.TimeEntry as TimeEntry, Work.TimeEntry.Company as Company, Work.TimeEntry.Customer as Customer,
	Work.TimeEntry.Performer as Performer, Work.TimeEntry.Project as Project,
	Work.AmountOpeningBalance AmountOpeningBalance, Work.AmountReceipt as AmountReceipt, Work.AmountExpense as AmountExpense,
	Work.AmountClosingBalance as AmountClosingBalance,
	Work.QuantityOpeningBalance QuantityOpeningBalance, Work.QuantityReceipt as QuantityReceipt, Work.QuantityExpense as QuantityExpense,
	Work.QuantityClosingBalance as QuantityClosingBalance,
	Tasks.Description as TaskDescription, Rates.HourlyRate as HourlyRate,
	isnull ( Tasks.RowKey, Items.RowKey ) as RowKey,
	Items.Capacity as Capacity, Items.Feature as Feature, Items.Package as Package, Items.Series as Series,
	isnull ( Tasks.LineNumber, Items.LineNumber ) as LineNumber,
	case
		when Items.Item is not null then Items.Item
		when Tasks.Task = value ( Catalog.Tasks.EmptyRef )
			or Tasks.Task.Item = value ( Catalog.Items.EmptyRef ) then Constants.DefaultService
		else Tasks.Task.Item
	end as Item,
	case
		when cast (
			case
				when Items.Item is not null then Items.Item
				when Tasks.Task = value ( Catalog.Tasks.EmptyRef )
					or Tasks.Task.Item = value ( Catalog.Items.EmptyRef ) then Constants.DefaultService
				else Tasks.Task.Item
			end as Catalog.Items ).Service
		then value ( Enum.Tables.Services )
		else value ( Enum.Tables.Items )
	end as ItemType,
	case when Items.Warehouse is null
		or Items.Warehouse = value ( Catalog.Warehouses.EmptyRef ) then Work.TimeEntry.Warehouse
		else Items.Warehouse
	end as Warehouse
{select SecondPeriod, Recorder.*, Company.*, Customer.*, TimeEntry.*,
	AmountOpeningBalance, AmountReceipt, AmountExpense, AmountClosingBalance,
	QuantityOpeningBalance, QuantityReceipt, QuantityExpense, QuantityClosingBalance,
	TaskDescription, Item.*, HourlyRate, Performer.*, Project.*, ItemType, RowKey,
	Capacity, Feature.*, Package.*, Series.*, Warehouse.*, LineNumber}
from AccumulationRegister.Work.BalanceAndTurnovers ( {&amp;DateStart}, {&amp;DateEnd}, Auto, , {TimeEntry.* as TimeEntry,
	TimeEntry.Company.* as Company, TimeEntry.Customer.* as Customer,
	TimeEntry.Performer.* as Performer, TimeEntry.Project.* as Project} ) as Work
	//
	// Tasks
	//
	left join Document.TimeEntry.Tasks as Tasks
	on Tasks.Ref = Work.TimeEntry
	and Tasks.RowKey = Work.RowKey
	//
	// Items
	//
	left join Document.TimeEntry.Items as Items
	on Items.Ref = Work.TimeEntry
	and Items.RowKey = Work.RowKey
	//
	// Rates
	//
	left join InformationRegister.TimeEntryRates as Rates
	on Rates.TimeEntry = Work.TimeEntry
	and Rates.RowKey = Work.RowKey
	//
	// Constants
	//
	join Constants as Constants
	on true
{where
	case when Items.Warehouse is null
		or Items.Warehouse = value ( Catalog.Warehouses.EmptyRef ) then Work.TimeEntry.Warehouse
		else Items.Warehouse
	end as Warehouse}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
