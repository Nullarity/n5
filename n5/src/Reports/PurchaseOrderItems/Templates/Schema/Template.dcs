		<dataSource>DataSource1</dataSource>
		<query>//
// Balances
//
select Balances.PurchaseOrder as PurchaseOrder, Balances.RowKey as RowKey, Balances.QuantityBalance as Quantity, Balances.AmountBalance as Amount,
	Balances.PurchaseOrder.Warehouse as Warehouse
into Balances
from AccumulationRegister.PurchaseOrders.Balance ( {&amp;Asof}, {PurchaseOrder.* as PurchaseOrder} ) as Balances
{where Balances.PurchaseOrder.Warehouse.* as Warehouse}
index by PurchaseOrder, RowKey
;
//
// Main selection
//
select Items.Item as Item, false as Service, Items.Feature as Feature, Items.Package as Package, Items.QuantityPkg as QuantityPkgOrdered, Items.Quantity as QuantityOrdered,
	Balances.RowKey as RowKey, Items.DocumentOrder as DocumentOrder, Items.DocumentOrderRowKey as DocumentOrderRowKey,
	Items.Price as Price, Items.Prices as Prices, "" as Description,
	case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end as DeliveryDate,
	case when Items.DocumentOrder refs Document.SalesOrder then cast ( Items.DocumentOrder as Document.SalesOrder ).Customer else cast ( Items.DocumentOrder as Document.InternalOrder ).Department end as Customer,
	Balances.PurchaseOrder as PurchaseOrder, Balances.Quantity as Quantity,
	Balances.Quantity / Items.Capacity as QuantityPkg, Balances.Amount as Amount,
	Items.Quantity - Balances.Quantity as QuantitySold,
	Items.QuantityPkg - Balances.Quantity / Items.Capacity as QuantityPkgSold,
	Items.VATCode as VATCode, Items.VATRate as VATRate, Items.Capacity as Capacity, Balances.Warehouse as Warehouse, Items.DiscountRate as DiscountRate,
	case when Items.Quantity = Balances.Quantity then Items.Discount else Items.Amount / 100 * Items.DiscountRate end as Discount
{select Item.*, Feature.*, Package.*, QuantityPkgOrdered, QuantityOrdered, Quantity, RowKey, DocumentOrder.*, DocumentOrderRowKey, Price, Prices, DeliveryDate, PurchaseOrder.*, Quantity, QuantityPkg,
	Amount, Customer.*, QuantitySold, QuantityPkgSold, VATCode.*, VATRate, Capacity, Warehouse.*, DiscountRate, Discount, Description}
from Document.PurchaseOrder.Items as Items
	//
	// Balances
	//
	join Balances as Balances
	on Balances.PurchaseOrder = Items.Ref
	and Balances.RowKey = Items.RowKey
{where Items.Item.* as Item, false as Service, Items.Feature.* as Feature, Items.Package.* as Package, Items.QuantityPkg as QuantityPkgOrdered, Items.DocumentOrder.* as DocumentOrder,
	Items.Price as Price, Items.Prices.* as Prices,
	case when Items.DocumentOrder refs Document.SalesOrder then cast ( Items.DocumentOrder as Document.SalesOrder ).Customer else cast ( Items.DocumentOrder as Document.InternalOrder ).Department end as Customer,
	Balances.Quantity / Items.Capacity as QuantityPkg, Balances.Amount as Amount,
	Items.QuantityPkg - Balances.Quantity / Items.Capacity as QuantityPkgSold,
	case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end between &amp;DeliveryDateStart and &amp;DeliveryDateEnd}
union all
select Services.Item, true, Services.Feature, null, Services.Quantity, Services.Quantity, Balances.RowKey, Services.DocumentOrder, Services.DocumentOrderRowKey,
	Services.Price, Services.Prices, Services.Description,
	case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end,
	Services.DocumentOrder.Department, Balances.PurchaseOrder, Balances.Quantity, Balances.Quantity,
	Balances.Amount, Services.Quantity - Balances.Quantity, Services.Quantity - Balances.Quantity,
	Services.VATCode, Services.VATRate, 1, Balances.Warehouse, Services.DiscountRate,
	case when Services.Quantity = Balances.Quantity then Services.Discount else Services.Amount / 100 * Services.DiscountRate end
from Document.PurchaseOrder.Services as Services
	//
	// Balances
	//
	join Balances as Balances
	on Balances.PurchaseOrder = Services.Ref
	and Balances.RowKey = Services.RowKey
{where Services.Item.* as Item, true as Service, Services.Feature.* as Feature, null as Package, Services.Quantity as QuantityPkgOrdered, Services.DocumentOrder.* as DocumentOrder,
	Services.Price as Price, Services.Prices.* as Prices, Services.DocumentOrder.Department.* as Customer, Balances.Quantity as QuantityPkg,
	Balances.Amount as Amount, Services.Quantity - Balances.Quantity as QuantityPkgSold,
	case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end between &amp;DeliveryDateStart and &amp;DeliveryDateEnd}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
