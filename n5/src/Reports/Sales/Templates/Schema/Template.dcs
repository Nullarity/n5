		<dataSource>DataSource1</dataSource>
		<query>//
// Warehouses
//
select allowed Warehouses.Ref as Warehouse
into Warehouses
from Catalog.Warehouses as Warehouses
where not Warehouses.DeletionMark
{where Warehouses.Ref.* as Warehouse, Warehouses.Owner.* as Company}
index by Warehouse
;
//
// Details
//
select distinct Details.Item as Item,  Details.Warehouse as Warehouse, Details.ItemKey as ItemKey,
	Details.Package as Package, Details.Feature as Feature, Details.Series as Series
into Details
from InformationRegister.ItemDetails as Details
where case when Item.Service then true else Details.Warehouse in ( select Warehouse from Warehouses ) end
{where Details.Warehouse.* as Warehouse, Details.Item.* as Item, Details.Package.* as Package,
Details.Feature.* as Feature, Details.Series.* as Series}
index by ItemKey
;
//
// Customers
//
select allowed Customers.Ref as Customer
into Customers
from Catalog.Organizations as Customers
where not Customers.DeletionMark
and Customers.Customer
{where Customers.Ref.* as Customer}
index by Customer
;
//
// Main Selection
//
select Turnovers.Department as Department, Details.Item as Item,  Details.Warehouse as Warehouse, 
	Turnovers.AmountTurnover as Total, Turnovers.AmountTurnover - Turnovers.VATTurnover as Amount,
	Turnovers.CostTurnover as Cost, Turnovers.VATTurnover as VAT,
	( Turnovers.AmountTurnover - Turnovers.VATTurnover ) - Turnovers.CostTurnover as Profit, 
	Turnovers.QuantityTurnover as Quantity, Details.ItemKey as ItemKey, Turnovers.Customer as Customer,
	case when Turnovers.CostTurnover = 0 then 100 else ( ( Turnovers.AmountTurnover - Turnovers.VATTurnover ) - Turnovers.CostTurnover ) * 100 / Turnovers.CostTurnover end as PercentProfit,
	Turnovers.MonthPeriod AS MonthPeriod, Turnovers.Department.Owner as Company, Turnovers.Recorder as Recorder,
	Details.Package as Package, Details.Feature as Feature, Details.Series as Series
{select Department.*, Item.*, Warehouse.*, Company, Total, Amount, Cost, Quantity, ItemKey,
	PercentProfit, Customer.*, Profit, MonthPeriod, Recorder.* , VAT, Package.*, Feature.*, Series.*}
from AccumulationRegister.Sales.Turnovers ( {&amp;DateStart}, {&amp;DateEnd}, auto, { Department.* as Department, 
	Department.Owner.* as Company, Customer.* as Customer } ) as Turnovers
	//
	// Details
	//
	join Details as Details
	on Details.ItemKey = Turnovers.ItemKey
	//
	// Filter by Customers
	//
	{join Customers as Customers
	on Customers.Customer = Turnovers.Customer}
{where Customers.Customer.* as Customer}
</query>
		<autoFillFields>false</autoFillFields>
