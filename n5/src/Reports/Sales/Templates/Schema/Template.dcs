	</totalField>
	<totalField>
		<dataPath>PercentProfit</dataPath>
		<expression>case when sum ( Cost ) = 0 then 100 else ( sum ( Amount ) - sum ( Cost ) ) * 100 / sum ( Cost ) end</expression>
	</totalField>
	<totalField>
		<dataPath>Quantity</dataPath>
		<expression>Sum(Quantity)</expression>
	</totalField>
	<totalField>
		<dataPath>Profit</dataPath>
		<expression>Sum(Profit)</expression>
	</totalField>
	<totalField>
		<dataPath>Total</dataPath>
		<expression>Sum(Total)</expression>
	</totalField>
	<totalField>
		<dataPath>VAT</dataPath>
		<expression>Sum(VAT)</expression>
	</totalField>
