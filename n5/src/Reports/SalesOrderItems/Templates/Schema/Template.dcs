		<dataSource>DataSource1</dataSource>
		<query>//
// Vendors &amp; Items
//
select distinct &amp;Vendor as Vendor, Items.Item as Item
into VendorItems
from InformationRegister.VendorItems as Items
index by Vendor, Item
{where Items.Vendor = &amp;Vendor}
;
//
// DepartmentItems &amp; Items
//
select distinct &amp;Department as Department, Items.Item as Item
into DepartmentItems
from InformationRegister.DepartmentItems as Items
index by Department, Item
{where Items.Department = &amp;Department}
;
//
// Balances
//
select Balances.RowKey as RowKey, Balances.SalesOrder as SalesOrder, Balances.SalesOrder.Warehouse as Warehouse,
	Balances.QuantityBalance as Quantity, Balances.AmountBalance as Amount
into Balances
from AccumulationRegister.SalesOrders.Balance ( {&amp;Asof}, {SalesOrder.* as SalesOrder} ) as Balances
{where Balances.SalesOrder.Warehouse.* as Warehouse}
index by SalesOrder, RowKey
;
//
// Items and Services
//
select Items.Ref as SalesOrder, Items.RowKey as RowKey, Items.Item as Item, false as Service,
	Items.Package as Package, Items.Capacity as Capacity, Items.Feature as Feature, Items.QuantityPkg as QuantityPkgOrdered,
	Items.Quantity as QuantityOrdered, Items.Price as Price, Items.Prices as Prices,
	case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end as DeliveryDate,
	Items.DiscountRate as DiscountRate,	Items.VATCode as VATCode, Items.VATRate as VATRate, 
	Balances.Warehouse as Warehouse, Balances.Quantity as Quantity, Balances.Amount as Amount,
	case when Items.Quantity = Balances.Quantity then Items.Discount else Items.Amount / 100 * Items.DiscountRate end as Discount,
	"" as Description
into Items
from Document.SalesOrder.Items as Items
	//
	// Balances
	//
	join Balances as Balances
	on Balances.SalesOrder = Items.Ref
	and Balances.RowKey = Items.RowKey
{where Items.Item.* as Item, false as Service, Items.Package.* as Package,
Items.Feature.* as Feature, Balances.Amount as Amount, Items.Price as Price,
Items.Prices.* as Prices, 
case when Items.DeliveryDate = datetime ( 1, 1, 1 ) then Items.Ref.DeliveryDate else Items.DeliveryDate end between &amp;DeliveryStart and &amp;DeliveryEnd}
union all
select Services.Ref, Services.RowKey, Services.Item, true, null, 1, Services.Feature,
	Services.Quantity, Services.Quantity, Services.Price, Services.Prices, 
	case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end, 
	Services.DiscountRate, Services.VATCode, Services.VATRate, 
	Balances.Warehouse, Balances.Quantity, Balances.Amount,
	case when Services.Quantity = Balances.Quantity then Services.Discount else Services.Amount / 100 * Services.DiscountRate end,
	Services.Description
from Document.SalesOrder.Services as Services
	//
	// Balances
	//
	join Balances as Balances
	on Balances.SalesOrder = Services.Ref
	and Balances.RowKey = Services.RowKey
{where Services.Item.* as Item, true as Service, null as Package, Services.Feature.* as Feature,
Services.Price as Price, Balances.Amount as Amount, Services.Prices.* as Prices,
case when Services.DeliveryDate = datetime ( 1, 1, 1 ) then Services.Ref.DeliveryDate else Services.DeliveryDate end between &amp;DeliveryStart and &amp;DeliveryEnd}
index by Warehouse, SalesOrder, RowKey, Item, Package, Feature
;
//
// PurchaseOrders
//
select PurchaseOrders.Ref as PurchaseOrder, PurchaseOrders.DocumentOrder as SalesOrder,
	PurchaseOrders.RowKey as RowKey,
	PurchaseOrders.DocumentOrderRowKey as SalesOrderRowKey
into PurchaseOrders
from Document.PurchaseOrder.Items as PurchaseOrders
	//
	// Items
	//
	join Items as Items
	on PurchaseOrders.DocumentOrder = Items.SalesOrder
	and PurchaseOrders.DocumentOrderRowKey = Items.RowKey
index by PurchaseOrder, RowKey, DocumentOrder, DocumentOrderRowKey
;
//
// ProductionOrders
//
select ProductionOrders.Ref as ProductionOrder, ProductionOrders.DocumentOrder as SalesOrder,
	ProductionOrders.RowKey as RowKey,
	ProductionOrders.DocumentOrderRowKey as SalesOrderRowKey
into ProductionOrders
from Document.ProductionOrder.Items as ProductionOrders
	//
	// Items
	//
	join Items as Items
	on ProductionOrders.DocumentOrder = Items.SalesOrder
	and ProductionOrders.DocumentOrderRowKey = Items.RowKey
index by ProductionOrder, RowKey, DocumentOrder, DocumentOrderRowKey
;
select PurchaseOrders.SalesOrder as SalesOrder, PurchaseOrders.SalesOrderRowKey as SalesOrderRowKey,
	Purchases.QuantityBalance as QuantityAllocated
into Allocated
from AccumulationRegister.PurchaseOrders.Balance ( {&amp;Asof},
	( PurchaseOrder, RowKey ) in ( select distinct PurchaseOrder, RowKey from PurchaseOrders ) ) as Purchases
	//
	// PurchaseOrders
	//
	join PurchaseOrders as PurchaseOrders
	on PurchaseOrders.PurchaseOrder = Purchases.PurchaseOrder
	and PurchaseOrders.RowKey = Purchases.RowKey
union
select ProductionOrders.SalesOrder, ProductionOrders.SalesOrderRowKey, Production.QuantityBalance
from AccumulationRegister.ProductionOrders.Balance ( {&amp;Asof},
	( ProductionOrder, RowKey ) in ( select distinct ProductionOrder, RowKey from ProductionOrders ) ) as Production
	//
	// ProductionOrders
	//
	join ProductionOrders as ProductionOrders
	on ProductionOrders.ProductionOrder = Production.ProductionOrder
	and ProductionOrders.RowKey = Production.RowKey	
index by SalesOrder, SalesOrderRowKey
;
//
// Main selection
//
select Items.Item as Item, Items.Service as Service, Items.Package as Package,
	Items.Feature as Feature, Items.RowKey as RowKey, Items.Description as Description,
	Items.SalesOrder as SalesOrder, Items.Warehouse as Warehouse, Items.DeliveryDate as DeliveryDate,
	Items.Price as Price, Items.Prices as Prices, Items.Capacity as Capacity,
	Items.QuantityPkgOrdered as QuantityPkgOrdered, Items.QuantityOrdered as QuantityOrdered,
	Items.Quantity / Items.Capacity as QuantityPkg,
	Items.Quantity as Quantity, Items.Amount as Amount, Items.DiscountRate as DiscountRate,
	Items.Discount as Discount,	( Items.QuantityOrdered - Items.Quantity ) as QuantitySold,
	( Items.QuantityPkgOrdered - ( Items.Quantity / Items.Capacity ) ) as QuantityPkgSold,
	Reserves.QuantityBalance / Items.Capacity as QuantityPkgReserve,
	Reserves.QuantityBalance as QuantityReserve,
	Allocation.QuantityBalance / Items.Capacity as QuantityPkgForAllocation,
	Allocation.QuantityBalance as QuantityForAllocation,
	Allocated.QuantityAllocated / Items.Capacity as QuantityPkgAllocated,
	Allocated.QuantityAllocated as QuantityAllocated, Items.VATCode as VATCode, Items.VATRate as VATRate 
{select SalesOrder.*, Warehouse.*, Item.*, DeliveryDate, Service, Package.*, Capacity,
	DiscountRate, Discount, Feature.*, QuantityOrdered, QuantityPkgOrdered,
	QuantitySold, QuantityPkgSold, Price, QuantityPkg, Amount, Quantity, QuantityReserve,
	QuantityPkgReserve, QuantityForAllocation, QuantityPkgForAllocation, QuantityAllocated,
	QuantityPkgAllocated, RowKey, Prices.*, VATCode.*, VATRate, Description}
from Items as Items
	//
	// Reserves
	//
	{left join AccumulationRegister.Reserves.Balance ( {&amp;Asof},
		( DocumentOrder, RowKey ) in ( select distinct SalesOrder, RowKey from Items ) ) as Reserves
	on Reserves.DocumentOrder = Items.SalesOrder
	and Reserves.RowKey = Items.RowKey}
	//
	// Allocation
	//
	{left join AccumulationRegister.Allocation.Balance ( {&amp;Asof},
		( DocumentOrder, RowKey ) in ( select distinct SalesOrder, RowKey from Items ) ) as Allocation
	on Allocation.DocumentOrder = Items.SalesOrder
	and Allocation.RowKey = Items.RowKey}
	//
	// Allocated
	//
	{left join Allocated as Allocated
	on Allocated.SalesOrder = Items.SalesOrder
	and Allocated.SalesOrderRowKey = Items.RowKey}
	//
	// Filter by VendorItems
	//
	{join VendorItems as VendorItems
	on VendorItems.Item = Items.Item}
	//
	// Filter by DepartmentItems
	//
	{join DepartmentItems as DepartmentItems
	on DepartmentItems.Item = DepartmentItems.Item}
{where Items.QuantityPkgOrdered as QuantityPkgOrdered,
Items.Quantity / Items.Capacity as QuantityPkg,
( Items.QuantityPkgOrdered - ( Items.Quantity / Items.Capacity ) ) as QuantityPkgSold,
Reserves.QuantityBalance / Items.Capacity as QuantityPkgReserve,
Allocation.QuantityBalance / Items.Capacity as QuantityPkgForAllocation,
Allocated.QuantityAllocated / Items.Capacity as QuantityPkgAllocated,
VendorItems.Vendor = &amp;Vendor, DepartmentItems.Department = &amp;Department}
</query>
