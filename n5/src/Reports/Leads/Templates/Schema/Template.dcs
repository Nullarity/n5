		<field xsi:type="DataSetFieldField">
			<dataPath>Source</dataPath>
			<field>Source</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>select Leads.Ref as Ref, Leads.Campaign as Campaign, Leads.Company as Company, 1 as Count,
	Leads.Responsible as Responsible, Leads.Industry as Industry, Leads.Source as Source
{select Ref.*, Campaign.*, Company.*, Count, Responsible.*, Industry.*, Source.*}
from Catalog.Leads as Leads
where not Leads.DeletionMark
{where Leads.Date between &amp;DateStart and &amp;DateEnd, Leads.Ref.* as Ref, Leads.Campaign.* as Campaign,
Leads.Company.* as Company, Leads.Responsible.* as Responsible, Leads.Industry.* as Industry,
Leads.Source.* as Source}
</query>
