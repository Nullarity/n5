		<dataSource>DataSource1</dataSource>
		<query>// All Discounts
select allowed Scale.Ref as Ref, Scale.Begin as Begin, Scale.End as End, Scale.Percent as Percent,
	case when Scale.Start &lt; &amp;DateStart then &amp;DateStart else Scale.Start end as Start,
	case when Scale.Finish &gt; &amp;DateEnd then &amp;DateEnd else Scale.Finish end as Finish,
	Items.Ref as Item
into Discounts
from (
	select Scale.Ref as Ref, Scale.Ref.Date as Start,
		case when Scale.Ref.To = datetime ( 1, 1, 1 ) then datetime ( 3999, 12, 31 ) else Scale.Ref.To end as Finish,
		Scale.From as Begin, Scale.To as End, Scale.Percent as Percent
	from Document.Discounts.Scale as Scale
	where Scale.Ref.Posted
	and Scale.Ref.Date between &amp;DateStart and &amp;DateEnd
	{where Scale.Ref.Company.* as Company}
	union
	select Scale.Ref, Scale.Ref.Date, Scale.Ref.To, Scale.From, Scale.To, Scale.Percent
	from Document.Discounts.Scale as Scale
	where Scale.Ref.Posted
	and Scale.Ref.To between &amp;DateStart and &amp;DateEnd
	{where Scale.Ref.Company.* as Company}
	union
	select Scale.Ref, Scale.Ref.Date, datetime ( 3999, 12, 31 ), Scale.From, Scale.To, Scale.Percent
	from Document.Discounts.Scale as Scale
	where Scale.Ref.Posted
	and Scale.Ref.To = datetime ( 1, 1, 1 )
	{where Scale.Ref.Company.* as Company}
	) as Scale
	//
	// Items and groups
	//
	join Document.Discounts.Items as ItemsAndGroups
	on ItemsAndGroups.Ref = Scale.Ref
	//
	// Items
	//
	join Catalog.Items as Items
	on Items.Ref = ItemsAndGroups.Item
	or Items.Parent = ItemsAndGroups.Item
	or Items.Parent.Parent = ItemsAndGroups.Item
	or Items.Parent.Parent.Parent = ItemsAndGroups.Item
	or Items.Parent.Parent.Parent.Parent = ItemsAndGroups.Item
	or Items.Parent.Parent.Parent.Parent.Parent = ItemsAndGroups.Item
	or Items.Parent.Parent.Parent.Parent.Parent.Parent = ItemsAndGroups.Item
;
// Spans
select Discounts.Item as Item, Discounts.Ref as Ref, Discounts.Start as Start,
	min ( case when Closing.Start is null then Discounts.Finish
				when Closing.Start &gt; Discounts.Finish then Discounts.Finish
				else dateadd ( Closing.Start, second, -1 )
				end
		) as Finish
into Spans
from Discounts as Discounts
	//
	// Closing periods
	//
	left join Discounts as Closing
	on Closing.Start &gt; Discounts.Start
	and Closing.Item = Discounts.Item
group by Discounts.Item, Discounts.Start, Discounts.Ref
;
// Spans + Tails
select Spans.Item as Item, Spans.Ref as Ref, Spans.Start as Start, Spans.Finish as Finish
into Intervals
from Spans as Spans
union
select Tails.Item, Discounts.Ref, Tails.Start, Tails.Finish
from (
	select Tails.Item as Item, Tails.Start as Start, Tails.Finish as Finish,
		Tails.StartReference as StartReference
	from (
		select Tails.Item as Item,
			max ( dateadd ( Spans.Finish, second, 1 ) ) as Start,
			max ( Tails.Finish ) as Finish,
			max ( Discounts.Start ) as StartReference
		from Discounts as Tails
			//
			// Tails
			//
			join Spans as Spans
			on Spans.Finish &gt; Tails.Start
			and Spans.Finish &lt; Tails.Finish
			and Spans.Item = Tails.Item
			//
			// Last references
			//
			join Discounts as Discounts
			on Discounts.Item = Tails.Item
			and Discounts.Finish = Tails.Finish
		group by Tails.Item
		) as Tails
	) as Tails
	//
	// Closest references
	//
	join Discounts as Discounts
	on Discounts.Item = Tails.Item
	and Discounts.Start = Tails.StartReference
	and Discounts.Finish = Tails.Finish
;
// Item Discounts
select Discounts.Item as Item, Discounts.Begin as Begin, Discounts.End as End, Discounts.Percent as Percent,
	Intervals.Start as Start, Intervals.Finish as Finish, Discounts.Ref as Ref
into Rebate
from Discounts as Discounts
	//
	// Intervals
	//
	join Intervals as Intervals
	on Intervals.Item = Discounts.Item
	and Intervals.Ref = Discounts.Ref
;
// Discount Item Keys 
select Details.ItemKey as ItemKey
into DiscountItems
from InformationRegister.ItemDetails as Details
{where Details.Item.* as DiscountItem}
;
// Invoices
select allowed Sales.Customer as Customer, Sales.ItemKey as ItemKey, Sales.Period as Period,
	Sales.Recorder as Recorder, Sales.AmountTurnover as Amount,
	Items.Item as Item
into Invoices
from AccumulationRegister.Sales.Turnovers ( &amp;DateStart, &amp;DateEnd, recorder,
	ItemKey not in ( select ItemKey from DiscountItems )
	{Department.Owner.* as Company, Customer.* as Customer} ) as Sales
	//
	// Items
	//
	join InformationRegister.ItemDetails as Items
	on Items.ItemKey = Sales.ItemKey
	//
	// Allowed customers
	//
	join Catalog.Organizations as Customers
	on Customers.Ref = Sales.Customer
index by ItemKey
;
// Sales
select Sales.Customer as Customer, sum ( Sales.Amount ) as Amount,
	case when sum ( Sales.Amount ) &gt; Discounts.Begin
			and sum ( Sales.Amount ) &lt;= Discounts.End then Discounts.Percent
		else 0
	end as Percent,
	Discounts.Ref as Ref
into Sales
from Invoices as Sales
	//
	// Discounts
	//
	join Rebate as Discounts
	on Discounts.Item = Sales.Item
	and Sales.Period between Discounts.Start and Discounts.Finish
group by Sales.Customer, Discounts.Begin, Discounts.End, Discounts.Percent, Discounts.Ref
having
	case when sum ( Sales.Amount ) &gt; Discounts.Begin
			and sum ( Sales.Amount ) &lt;= Discounts.End then Discounts.Percent
		else 0
	end &gt; 0
;
// Main selection
select Sales.Customer as Customer, Sales.Amount as Sold, Sales.Percent as Percent,
	cast ( Sales.Amount / 100 * Sales.Percent as Number ( 15, 2 ) ) as Discount,
	Sales.Ref as Ref,
	DiscountItems.ItemKey, Invoices.Recorder as Recorder
{select Customer.*, Sold, Percent, Discount, Ref.*}
from Sales as Sales
	//
	// Forcibly select fields from DiscountItems
	//
	left join DiscountItems as DiscountItems
	on false
	//
	// Forcibly select fields from Invoices
	//
	left join Invoices as Invoices
	on false
{where cast ( Sales.Amount / 100 * Sales.Percent as Number ( 15, 2 ) ) as Discount}</query>
