		<dataSource>DataSource1</dataSource>
		<query>// Invoices
select Sales.Customer as Customer, Sales.Recorder as Recorder, Sales.Item as Item,
	Sales.Amount as Amount, Discounts.Ref as Ref, Discounts.Percent as Percent,
	Sales.Amount / 100 * Discounts.Percent as ItemDiscount
{select Customer.*, Recorder.*, Item.*, Amount, Ref.*, Percent, ItemDiscount}
from Invoices as Sales
	//
	// Discounts
	//
	join Discounts as Discounts
	on Discounts.Item = Sales.Item
	and true
	//
	// Intervals
	//
	join Intervals as Intervals
	on Intervals.Item = Sales.Item
	and Intervals.Ref = Discounts.Ref
	and Sales.Period between Intervals.Start and Intervals.Finish</query>
		<autoFillFields>false</autoFillFields>
