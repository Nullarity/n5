		<dataSource>DataSource1</dataSource>
		<query>//
// Projects
//
select allowed Projects.Ref as Project
into Projects
from Catalog.Projects as Projects
where not Projects.DeletionMark
and not Projects.IsFolder
and Projects.ProjectType not in (
		value ( Enum.ProjectTypes.SickDays ),
		value ( Enum.ProjectTypes.Vacations ),
		value ( Enum.ProjectTypes.Holidays ) )
{where Projects.Currency = &amp;Currency, Projects.Currency.* as Currency, Projects.Owner.* as Customer, Projects.Ref.* as Project}
index by Project
;
//
// Tasks
//
select Tasks.Ref as Project, Tasks.Task as Task, Tasks.Employee as Employee, Tasks.TimeType as TimeType, Tasks.HourlyCost as HourlyCost,
	sum ( Tasks.Minutes ) as Minutes, sum ( Tasks.Amount ) as Amount
into Tasks
from Catalog.Projects.Tasks as Tasks
	//
	// Filter by Projects
	//
	join Projects as Projects
	on Projects.Project = Tasks.Ref
group by Tasks.Ref, Tasks.Task, Tasks.Employee, Tasks.TimeType, Tasks.HourlyCost
{where Tasks.Employee.* as Employee, Tasks.Task.* as Task}
index by Project, Task
;
//
// Employees
//
select allowed Employees.Ref as Employee
into Employees
from Catalog.Employees as Employees
index by Employee
;
//
// Main selection
//
select allowed Projects.Project.Owner as Customer, Projects.Project as Project, Projects.Employee as Employee, Projects.Task as Task,
	case when ( Projects.Project.DateStart = datetime ( 1, 1, 1 ) or Projects.Project.DateEnd = datetime ( 1, 1, 1 ) ) then 0
		else 1 + datediff ( Projects.Project.DateStart, Projects.Project.DateEnd, day )
	end as ProjectDuration,
	sum ( Projects.EstimatedMinutes ) as EstimatedMinutes, sum ( Projects.EstimatedCost ) as EstimatedCost,
	sum ( Projects.EstimatedBillableMinutes ) as EstimatedBillableMinutes, sum ( Projects.EstimatedNonBillableMinutes ) as EstimatedNonBillableMinutes,
	sum ( Projects.ActualBillableMinutes ) as ActualBillableMinutes, sum ( Projects.ActualNonBillableMinutes ) as ActualNonBillableMinutes,
	sum ( Projects.ActualMinutes ) as ActualMinutes, sum ( Projects.ActualCost ) as ActualCost, sum ( Projects.ProjectAmount ) as ProjectAmount,
	case when ( sum ( Projects.EstimatedMinutes ) - sum ( Projects.ActualMinutes ) ) &gt;= 0 then sum ( Projects.EstimatedMinutes ) - sum ( Projects.ActualMinutes ) else 0 end as MinutesRemain,
	case when ( sum ( Projects.EstimatedMinutes ) - sum ( Projects.ActualMinutes ) ) &lt; 0 then sum ( Projects.ActualMinutes ) - sum ( Projects.EstimatedMinutes ) else 0 end as MinutesOver,
	case when ( sum ( Projects.EstimatedCost ) - sum ( Projects.ActualCost ) ) &gt;= 0 then sum ( Projects.EstimatedCost ) - sum ( Projects.ActualCost ) else 0 end as CostRemain,
	case when ( sum ( Projects.EstimatedCost ) - sum ( Projects.ActualCost ) ) &lt; 0 then sum ( Projects.ActualCost ) - sum ( Projects.EstimatedCost ) else 0 end as CostOver,
	case when Project.DateEnd = datetime ( 1, 1, 1 ) then 0
		else
			case when datediff ( case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, Project.DateEnd, day ) &gt;= 0 then datediff ( case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, Project.DateEnd, day )
				else 0
			end
	end as DaysRemain,
	case when Project.DateEnd = datetime ( 1, 1, 1 ) then 0
		else
			case when datediff ( case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, Project.DateEnd, day ) &lt; 0 then datediff ( Project.DateEnd, case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, day )
				else 0
			end
	end as DaysOver,
	Projects.Recorder as Recorder
{select Customer.*, Project.*, Employee.*, Task.*, EstimatedMinutes, EstimatedCost, EstimatedBillableMinutes, EstimatedNonBillableMinutes,
	ActualMinutes, ActualBillableMinutes, ActualNonBillableMinutes, ActualCost, ProjectAmount, MinutesRemain, MinutesOver, CostRemain,
	CostOver, DaysRemain, DaysOver, ProjectDuration, Recorder.*}
from (
	//
	// Projects
	//
	select Projects.Ref as Project, ProjectTasks.Employee as Employee, isnull ( ProjectTasks.Task, value ( Catalog.Tasks.EmptyRef ) ) as Task,
		case when Projects.Pricing = value ( Enum.ProjectsPricing.Amount ) then Projects.Minutes else ProjectTasks.Minutes end as EstimatedMinutes,
		case when Projects.Pricing = value ( Enum.ProjectsPricing.Amount ) then Projects.Cost else ProjectTasks.Minutes / 60 * ProjectTasks.HourlyCost end as EstimatedCost,
		case when ProjectTasks.TimeType = value ( Enum.Time.Billable ) then ProjectTasks.Minutes else 0 end as EstimatedBillableMinutes,
		case when ProjectTasks.TimeType = value ( Enum.Time.NonBillable ) then ProjectTasks.Minutes else 0 end as EstimatedNonBillableMinutes,
		0 as ActualMinutes, 0 as ActualBillableMinutes, 0 as ActualNonBillableMinutes, 0 as ActualCost,
		case when Projects.Pricing = value ( Enum.ProjectsPricing.Amount ) then Projects.Amount else ProjectTasks.Amount end as ProjectAmount,
		null as Recorder
		{select Recorder.*}
	from Catalog.Projects as Projects
		//
		// Filter by Projects
		//
		join Projects as ProjectsFilter
		on ProjectsFilter.Project = Projects.Ref
		//
		// ProjectTasks
		//
		left join Tasks as ProjectTasks
		on ProjectTasks.Project = Projects.Ref
		//
		// Filter by Employees
		//
		join Employees as Employees
		on Employees.Employee = ProjectTasks.Employee
		or ProjectTasks.Employee = value ( Catalog.Employees.EmptyRef )
	{where null as Recorder}
	union all
	//
	// ProjectsCost
	//
	select ProjectsCost.Project, ProjectsCost.Employee, ProjectsCost.Task, 0, 0, 0, 0, ProjectsCost.BillableMinutesTurnover + ProjectsCost.NonBillableMinutesTurnover,
		ProjectsCost.BillableMinutesTurnover, ProjectsCost.NonBillableMinutesTurnover, ProjectsCost.CostTurnover, 0, Recorder
	from AccumulationRegister.ProjectsCost.Turnovers ( , &amp;CurrentDate, auto, Employee in ( select Employee from Employees )
		{Project.Currency = &amp;Currency, Project.Currency.* as Currency, Project.Owner.* as Customer, Project.*,
		Employee.* as Employee, Task.* as Task} ) as ProjectsCost
		//
		// Filter by Projects
		//
		join Projects as Projects
		on Projects.Project = ProjectsCost.Project
	{where Recorder.* as Recorder}
	) as Projects
group by Projects.Project, Projects.Employee, Projects.Task, Projects.Recorder
{
where sum ( Projects.EstimatedMinutes ) as EstimatedMinutes, sum ( Projects.EstimatedCost ) as EstimatedCost,
	sum ( Projects.ActualMinutes ) as ActualMinutes, sum ( Projects.ActualCost ) as ActualCost, sum ( Projects.ProjectAmount ) as ProjectAmount,
	case when ( sum ( Projects.EstimatedMinutes ) - sum ( Projects.ActualMinutes ) ) &gt;= 0 then sum ( Projects.EstimatedMinutes ) - sum ( Projects.ActualMinutes ) else 0 end as MinutesRemain,
	case when ( sum ( Projects.EstimatedMinutes ) - sum ( Projects.ActualMinutes ) ) &lt; 0 then sum ( Projects.ActualMinutes ) - sum ( Projects.EstimatedMinutes ) else 0 end as MinutesOver,
	case when ( sum ( Projects.EstimatedCost ) - sum ( Projects.ActualCost ) ) &gt;= 0 then sum ( Projects.EstimatedCost ) - sum ( Projects.ActualCost ) else 0 end as CostRemain,
	case when ( sum ( Projects.EstimatedCost ) - sum ( Projects.ActualCost ) ) &lt; 0 then sum ( Projects.ActualCost ) - sum ( Projects.EstimatedCost ) else 0 end as CostOver,
	case when Project.DateEnd = datetime ( 1, 1, 1 ) then 0
		else
			case when datediff ( case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, Project.DateEnd, day ) &gt;= 0 then datediff ( case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, Project.DateEnd, day )
				else 0
			end
	end as DaysRemain,
	case when Project.DateEnd = datetime ( 1, 1, 1 ) then 0
		else
			case when datediff ( case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, Project.DateEnd, day ) &lt; 0 then datediff ( Project.DateEnd, case when Projects.Project.Completed then Projects.Project.CompletionDate else &amp;CurrentDate end, day )
				else 0
			end
	end as DaysOver
}</query>
