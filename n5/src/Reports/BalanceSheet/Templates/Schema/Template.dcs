				<dcscom:accountField>Account</dcscom:accountField>
			</role>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>select allowed Balances.DayPeriod as DayPeriod, Balances.MonthPeriod as MonthPeriod, Balances.SecondPeriod as SecondPeriod,
	case when ( Balances.Recorder = undefined ) then null else Balances.Recorder end as Recorder, Balances.Company as Company, Balances.Currency as Currency,
	Balances.Account as Account, Balances.ExtDimension1 as Dim1, Balances.ExtDimension2 as Dim2, Balances.ExtDimension3 as Dim3,
	Balances.AmountOpeningBalanceDr as AmountOpeningBalanceDr,
	Balances.AmountOpeningBalanceCr as AmountOpeningBalanceCr,
	Balances.AmountClosingBalanceDr as AmountClosingBalanceDr,
	Balances.AmountClosingBalanceCr as AmountClosingBalanceCr,
	Balances.AmountTurnoverDr as AmountTurnoverDr, Balances.AmountTurnoverCr as AmountTurnoverCr,
	Balances.QuantityOpeningBalanceDr as QuantityOpeningBalanceDr, Balances.QuantityOpeningBalanceCr as QuantityOpeningBalanceCr,
	Balances.QuantityTurnoverDr as QuantityTurnoverDr, Balances.QuantityTurnoverCr as QuantityTurnoverCr, Balances.QuantityClosingBalanceDr as QuantityClosingBalanceDr,
	Balances.QuantityClosingBalanceCr as QuantityClosingBalanceCr, Balances.CurrencyAmountOpeningBalanceDr as CurrencyAmountOpeningBalanceDr,
	Balances.CurrencyAmountOpeningBalanceCr as CurrencyAmountOpeningBalanceCr, Balances.CurrencyAmountTurnoverDr as CurrencyAmountTurnoverDr,
	Balances.CurrencyAmountTurnoverCr as CurrencyAmountTurnoverCr, Balances.CurrencyAmountClosingBalanceDr as CurrencyAmountClosingBalanceDr,
	Balances.CurrencyAmountClosingBalanceCr as CurrencyAmountClosingBalanceCr,
	case
		when Balances.Account.Type = value ( AccountType.Active ) and Balances.AmountOpeningSplittedBalanceDr &gt; 0 then
			Balances.AmountOpeningSplittedBalanceDr
		when Balances.Account.Type = value ( AccountType.Active ) and Balances.AmountOpeningSplittedBalanceCr &gt; 0 then
			- Balances.AmountOpeningSplittedBalanceCr
		when Balances.Account.Type = value ( AccountType.ActivePassive ) then Balances.AmountOpeningSplittedBalanceDr
		else
			0
	end as AmountOpeningSplittedBalanceDr,
	case
		when Balances.Account.Type = value ( AccountType.Passive ) and Balances.AmountOpeningSplittedBalanceCr &gt; 0 then
			Balances.AmountOpeningSplittedBalanceCr
		when Balances.Account.Type = value ( AccountType.Passive ) and Balances.AmountOpeningSplittedBalanceDr &gt; 0 then
			- Balances.AmountOpeningSplittedBalanceDr
		when Balances.Account.Type = value ( AccountType.ActivePassive ) then Balances.AmountOpeningSplittedBalanceCr
		else
			0
	end as AmountOpeningSplittedBalanceCr,
	case
		when Balances.Account.Type = value ( AccountType.Active ) and Balances.AmountClosingSplittedBalanceDr &gt; 0 then
			Balances.AmountClosingSplittedBalanceDr
		when Balances.Account.Type = value ( AccountType.Active ) and Balances.AmountClosingSplittedBalanceCr &gt; 0 then
			- Balances.AmountClosingSplittedBalanceCr
		when Balances.Account.Type = value ( AccountType.ActivePassive ) then Balances.AmountClosingSplittedBalanceDr
		else
			0
	end as AmountClosingSplittedBalanceDr,
	case
		when Balances.Account.Type = value ( AccountType.Passive ) and Balances.AmountClosingSplittedBalanceCr &gt; 0 then
			Balances.AmountClosingSplittedBalanceCr
		when Balances.Account.Type = value ( AccountType.Passive ) and Balances.AmountClosingSplittedBalanceDr &gt; 0 then
			- Balances.AmountClosingSplittedBalanceDr
		when Balances.Account.Type = value ( AccountType.ActivePassive ) then Balances.AmountClosingSplittedBalanceCr
		else
			0
	end as AmountClosingSplittedBalanceCr
{select DayPeriod, MonthPeriod, SecondPeriod, Recorder.*, Company.*, Currency.*, Account.*, Dim1.*, Dim2.*, Dim3.*,
	AmountOpeningBalanceDr, AmountOpeningBalanceCr, AmountClosingBalanceDr, AmountClosingBalanceCr, AmountTurnoverDr, AmountTurnoverCr, 
	QuantityOpeningBalanceDr, QuantityOpeningBalanceCr, QuantityTurnoverDr, QuantityTurnoverCr, QuantityClosingBalanceDr, QuantityClosingBalanceCr,
	CurrencyAmountOpeningBalanceDr, CurrencyAmountOpeningBalanceCr, CurrencyAmountTurnoverDr, CurrencyAmountTurnoverCr, CurrencyAmountClosingBalanceDr,
	CurrencyAmountClosingBalanceCr, AmountClosingSplittedBalanceCr, AmountClosingSplittedBalanceDr,
	AmountOpeningSplittedBalanceCr, AmountOpeningSplittedBalanceDr}
from AccountingRegister.General.BalanceAndTurnovers ( {&amp;DateStart}, {&amp;DateEnd}, Auto, ,
	{Account.*}, , {Company.* as Company, Currency.* as Currency,
	ExtDimension1.* as Dim1, ExtDimension2.* as Dim2, ExtDimension3.* as Dim3} ) as Balances
{where Recorder.* as Recorder}</query>
		<autoFillFields>false</autoFillFields>
