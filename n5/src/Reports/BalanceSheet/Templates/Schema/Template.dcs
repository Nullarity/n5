			<dataPath>AmountClosingSplittedBalanceDr</dataPath>
			<field>AmountClosingSplittedBalanceDr</field>
			<useRestriction>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<role>
				<dcscom:balance>true</dcscom:balance>
				<dcscom:balanceGroupName>AmountDr</dcscom:balanceGroupName>
