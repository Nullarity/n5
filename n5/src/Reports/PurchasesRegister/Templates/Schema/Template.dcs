		<dataSource>DataSource1</dataSource>
		<query>select VATPurchase.VAT as VAT, VATPurchase.Amount as Amount, VATPurchase.Ref as Ref,
	( VATPurchase.VAT + VATPurchase.Amount ) as Total, VATPurchase.Rate as Rate,
	VATPurchase.CodeFiscal as CodeFiscal, VATPurchase.Vendor as Vendor,
	case when VATPurchase.Date = datetime ( 1, 1, 1 ) then VATPurchase.DocumentDate else VATPurchase.Date end as Date, 
	case when VATPurchase.Number = "" then VATPurchase.DocumentNumber else VATPurchase.Number end as Number
{select VAT, Amount, Total, Rate.*, CodeFiscal, Vendor.*, Number, Date, Ref.*}
from (
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end as VAT, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end as Amount, 
		Table.VATCode as Rate, Table.Ref.Reference as Number, Table.Ref.ReferenceDate as Date, 
		Table.Ref.Vendor.CodeFiscal as CodeFiscal, Table.Ref.Vendor as Vendor, Table.Ref as Ref,
		Table.Ref.Date as DocumentDate, Table.Ref.Number as DocumentNumber
	from Document.VendorInvoice.Accounts as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and not Table.Ref.Import
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Reference, Table.Ref.ReferenceDate, 
		Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.VendorInvoice.FixedAssets as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and not Table.Ref.Import
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Reference, Table.Ref.ReferenceDate, 
		Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.VendorInvoice.IntangibleAssets as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and not Table.Ref.Import
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Reference, Table.Ref.ReferenceDate, 
		Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.VendorInvoice.Items as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and not Table.Ref.Import
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Reference, Table.Ref.ReferenceDate, 
		Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.VendorInvoice.Services as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and not Table.Ref.Import
	{where not &amp;Import}
	union all
	select - case when Table.Ref.Contract.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		- case when Table.Ref.Contract.Currency = &amp;Currency then Table.Amount - Table.VAT else cast ( ( Table.Amount - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Reference, Table.Ref.ReferenceDate, 
		Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.VendorInvoice.Discounts as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and not Table.Ref.Import
	and ( Table.Detail &lt;&gt; undefined or not Table.Document refs Document.PurchaseOrder )
	{where not &amp;Import}
	union all
	select Charges.VAT, Charges.Amount + Groups.Base, Table.Charge.VAT,
		case Table.Ref.Reference when "" then Table.Ref.Number else Table.Ref.Reference end,
		case Table.Ref.ReferenceDate when datetime ( 1, 1, 1 ) then Table.Ref.Date else Table.Ref.ReferenceDate end, Items.CodeFiscal, Items.Vendor, Table.Ref, null, null
	from Document.CustomsDeclaration.Charges as Table
		//
		// Custom Groups
		//
		join Document.CustomsDeclaration.CustomsGroups as Groups
		on Groups.Ref = Table.Ref
		and Groups.ID = Table.ID
		//
		// Invoice
		//
		left join (
			select Items.Invoice.Vendor.CodeFiscal as CodeFiscal, Items.Invoice.Vendor as Vendor, 
				Items.Ref as Ref, Items.ID as ID
			from Document.CustomsDeclaration.Items as Items
			where Items.Ref.Posted
			and Items.Ref.Date between &amp;DateStart and &amp;DateEnd
			and Items.Ref.Company = &amp;Company
			group by Items.Ref, Items.Invoice.Vendor, Items.ID 
				) as Items
		on Items.Ref = Table.Ref
		and Items.ID = Table.ID
		//
		// VAT
		//
		left join (
			select sum ( case when Table.VAT then Table.Amount else 0 end ) as VAT, 
				sum ( case when Table.VAT then 0 else Table.Amount end ) as Amount, Table.Ref as Ref,
				Table.ID as ID
			from Document.CustomsDeclaration.Charges as Table
			where Table.Ref.Posted
			and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
			and Table.Ref.Company = &amp;Company
			group by Table.Ref, Table.ID
			) as Charges
		on Charges.Ref = Table.Ref
		and Charges.ID = Table.ID
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.VAT
	{where &amp;Import}
	union all
	select Table.VAT, Table.Total - Table.VAT, Table.VATCode, Table.Number, Table.Date, 
		Table.Vendor.CodeFiscal, Table.Vendor, Table.Ref, null, null
	from Document.VATPurchases as Table
	where not Table.Ref.DeletionMark
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Number, Table.Date, Table.Vendor.CodeFiscal, Table.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.ExpenseReport.Items as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Type = value ( Enum.DocumentTypes.Invoice )
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Number, Table.Date, Table.Vendor.CodeFiscal, Table.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.ExpenseReport.Services as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Type = value ( Enum.DocumentTypes.Invoice )
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Number, Table.Date, Table.Vendor.CodeFiscal, Table.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.ExpenseReport.FixedAssets as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Type = value ( Enum.DocumentTypes.Invoice )
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Number, Table.Date, Table.Vendor.CodeFiscal, Table.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.ExpenseReport.IntangibleAssets as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Type = value ( Enum.DocumentTypes.Invoice )
	{where not &amp;Import}
	union all
	select case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Number, Table.Date, Table.Vendor.CodeFiscal, Table.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.ExpenseReport.Accounts as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	and Table.Type = value ( Enum.DocumentTypes.Invoice )
	{where not &amp;Import}
	union all
	select - case when Table.Ref.Currency = &amp;Currency then Table.VAT else cast ( Table.VAT * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		- case when Table.Ref.Currency = &amp;Currency then Table.Total - Table.VAT else cast ( ( Table.Total - Table.VAT ) * Table.Ref.Rate / Table.Ref.Factor as Number ( 15, 2 ) ) end, 
		Table.VATCode, Table.Ref.Reference, Table.Ref.ReferenceDate, 
		Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor, Table.Ref,
		Table.Ref.Date, Table.Ref.Number
	from Document.VendorReturn.Items as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.VATUse &lt;&gt; 0
	{where not &amp;Import}
	union all
	select Table.VATLocal, Table.AmountLocal - Table.VATLocal, Table.VATCode, Table.Ref.Reference,
		Table.Ref.ReferenceDate, Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor,
		Table.Ref, Table.Ref.Date, Table.Ref.Number
	from Document.AdjustVendorDebts.Accounting as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.ApplyVAT
	and Table.VATCode &lt;&gt; value ( Catalog.VAT.EmptyRef )
	and Table.Ref.Reference &lt;&gt; ""
	and Table.Ref.ReferenceDate &lt;&gt; datetime ( 1, 1, 1 )
	and Table.Ref.Option in (
		value ( Enum.AdjustmentOptions.CustomAccountDr ),
		value ( Enum.AdjustmentOptions.AccountingDr ),
		value ( Enum.AdjustmentOptions.AccountingCr )
	)
	{where not &amp;Import}
	union all
	select Table.VAT, Table.AmountLocal - Table.VATLocal, Table.VATCode, Table.Ref.Reference,
		Table.Ref.ReferenceDate, Table.Ref.Vendor.CodeFiscal, Table.Ref.Vendor,
		Table.Ref, Table.Ref.Date, Table.Ref.Number
	from Document.AdjustVendorDebts.Adjustments as Table
	where Table.Ref.Posted
	and Table.Ref.Date between &amp;DateStart and &amp;DateEnd
	and Table.Ref.Company = &amp;Company
	and Table.Ref.ApplyVAT
	and Table.VATCode &lt;&gt; value ( Catalog.VAT.EmptyRef )
	and Table.Ref.Reference &lt;&gt; ""
	and Table.Ref.ReferenceDate &lt;&gt; datetime ( 1, 1, 1 )
	and Table.Ref.Option in (
		value ( Enum.AdjustmentOptions.CustomAccountDr ),
		value ( Enum.AdjustmentOptions.AccountingDr ),
		value ( Enum.AdjustmentOptions.AccountingCr )
	)
	{where not &amp;Import}
) as VATPurchase</query>
