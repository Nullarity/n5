  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select TenantOrders.Ref, TenantOrders.DeletionMark, TenantOrders.Number, TenantOrders.Date, TenantOrders.Posted, TenantOrders.AccessPrice,
	TenantOrders.Amount, TenantOrders.Bonus, TenantOrders.BonusPercent, TenantOrders.Currency, TenantOrders.DateEnd, TenantOrders.Discount,
	TenantOrders.DiscountRate, TenantOrders.GrossAmount, TenantOrders.MonthsCount, TenantOrders.PromoCodeDiscount, TenantOrders.Tenant,
	TenantOrders.UsersCount, TenantOrders.PointInTime, TenantOrders.Tenant.PromoCode.Agent as Agent,
	isnull ( Balances.BonusBalance, 0 ) as Balance,
	case when isnull ( Balances.BonusBalance, 0 ) = 0 then true else false end as Paid
from Document.TenantOrder as TenantOrders
	//
	// Agents
	//
	left join AccumulationRegister.Agents.Balance as Balances
	on Balances.TenantOrder = TenantOrders.Ref
where TenantOrders.Bonus > 0</queryText>
      <mainTable>Document.TenantOrder</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
