    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select allowed TimeEntries.Ref, TimeEntries.DeletionMark, TimeEntries.Number, TimeEntries.Date, TimeEntries.Posted, TimeEntries.Appearance,
	TimeEntries.BillableMinutes, TimeEntries.Customer, TimeEntries.Creator, TimeEntries.Duration, TimeEntries.Employee, TimeEntries.Minutes,
	TimeEntries.Project, TimeEntries.Tasks, TimeEntries.PointInTime, TimeEntryInvoices.Amount as Amount, TimeEntries.Project.Currency as Curency,
	TimeEntries.Performer as Performer,
	case when TimeEntryInvoices.Invoice is null then true else false end as Invoiced,
	TasksList.Progress as TaskProgress, TasksList.Completed as TaskCompleted, TasksList.Status as TaskStatus, TasksList.Task as TaskRef, TasksList.Observation as Observation
from Document.TimeEntry as TimeEntries
	{left join InformationRegister.TimeEntryInvoices as TimeEntryInvoices
	on TimeEntryInvoices.TimeEntry = TimeEntries.Ref}
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as TasksList
	on TasksList.Source = TimeEntries.Ref
	and TasksList.User = &amp;Performer}</queryText>
      <mainTable>Document.TimeEntry</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
