    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>// Warning: Some fields are acquired through &quot;.&quot; (dot) instead of journal fields.
// The reason for this is because 1C will hide journal columns if user does not have
// access to all documents listed in the journal.
//
// The Company field (in the metadata structure) is used for RLS reasons only

select Journal.Ref as Ref, Journal.Date as Date, Journal.DeletionMark as DeletionMark,
	Journal.Posted as Posted, Journal.Type as Type, Journal.Ref.Creator as Creator,
	Journal.Ref.Company as Company, Journal.Ref.Memo as Memo
from DocumentJournal.Calculations as Journal</queryText>
      <mainTable>DocumentJournal.Calculations</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
