      <queryText>select Documents.Ref as Ref, Documents.Date as Date, Documents.DeletionMark as DeletionMark,
	Documents.Number as Number, Documents.Posted as Posted, Documents.Amount as Amount,
	Documents.Company as Company, Documents.Creator as Creator, Documents.Memo as Memo, Documents.Type as Type
from DocumentJournal.Salary as Documents
	//
	// Filter by Employee
	//
	{join (
		select &amp;Employee as Employee, Filter.Ref as Ref
		from FilterCriterion.Employee ( &amp;Employee ) as Filter
	) as Filter
	on Filter.Ref = Documents.Ref}
{where Filter.Employee = &amp;Employee}</queryText>
