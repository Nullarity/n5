        <expression>case Type
    when type ( &quot;Document.InvoicesReturn&quot; ) then format ( Amount, &quot;NZ=0&quot; ) + &quot; &quot; + &amp;Days
    when type ( &quot;Document.CreditLimit&quot; ) then
        case when Disable then &amp;DontControl else format ( Amount, &quot;NZ=0&quot; ) + &quot; &quot; + &amp;Currency end
    when type ( &quot;Document.SalesRestriction&quot; ) then
        case Amount when true then value ( Enum.AllowDeny.Deny ) else value ( Enum.AllowDeny.Allow ) end
    when type ( &quot;Document.SalesPermission&quot; ) then
        case when Waiting then &amp;Waiting else Resolution end
    when type ( &quot;Document.ChangesPermission&quot; ) then
        case when Waiting then &amp;Waiting else Resolution end
end</expression>
