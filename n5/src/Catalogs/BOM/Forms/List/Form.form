      <queryText>select BOM.Ref as Ref, BOM.DeletionMark as DeletionMark, BOM.Parent as Parent, BOM.IsFolder as IsFolder, BOM.Code as Code, BOM.Description as Description, BOM.Active as Active, BOM.Calculated as Calculated,
	BOM.Capacity as Capacity, BOM.Company as Company, BOM.Cost as Cost,	BOM.Created as Created, BOM.Creator as Creator,	BOM.Expense as Expense,	BOM.Feature as Feature,	BOM.Item as Item, BOM.Memo as Memo,
	BOM.Package as Package,	BOM.Precision as Precision,	BOM.Prices as Prices, BOM.Quantity as Quantity,	BOM.QuantityPkg as QuantityPkg,	BOM.Technologist as Technologist, BOM.Items as Items, BOM.Predefined as Predefined,
	BOM.PredefinedDataName as PredefinedDataName,
	TasksList.Progress as TaskProgress, TasksList.Completed as TaskCompleted, TasksList.Status as TaskStatus, TasksList.Task as TaskRef, TasksList.Observation as Observation
from Catalog.BOM as BOM
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as TasksList
	on TasksList.Source = BOM.Ref
	and TasksList.User = &amp;Performer}</queryText>
