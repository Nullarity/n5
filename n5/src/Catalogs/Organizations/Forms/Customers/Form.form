    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Customers.Ref, Customers.DeletionMark, Customers.Parent, Customers.IsFolder, Customers.Code,
	Customers.CodeFiscal as CodeFiscal, Customers.Description as Description, Customers.ApproveSalesOrders,
	Customers.Contact, Customers.Customer, Customers.CustomerContract, Customers.CustomerType, Customers.Email,
	Customers.Fax, Customers.Memo, Customers.FullDescription, Customers.PaymentAddress, Customers.PaymentContact,
	Customers.Phone, Customers.RegistrationDate, Customers.Salesman, Customers.ShippingAddress, Customers.ShippingContact,
	Customers.TaxCode, Customers.TaxGroup, Customers.Vendor, Customers.VendorContract, Customers.Web,
	Customers.ApprovalList, Customers.Predefined, Customers.PredefinedDataName, Debts.AmountBalance as Debt,
	Tasks.Progress as Progress, Tasks.Completed as Completed, Tasks.Task as TaskRef, Tasks.Status as Status
from Catalog.Organizations as Customers
	//
	// Debts
	//
	{left join AccumulationRegister.Debts.Balance ( , {Contract.* as Contract} ) as Debts
	on Debts.Contract = Customers.CustomerContract}
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as Tasks
	on Tasks.Source = Customers.Ref
	and Tasks.User = &amp;Performer}</queryText>
