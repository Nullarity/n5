      <queryText>select Holidays.Reference as Reference, Holidays.Day as Day, Holidays.Description as Description,
	case when weekday ( Holidays.Day ) = 1 then value ( Enum.Week.Monday )
		when weekday ( Holidays.Day ) = 2 then value ( Enum.Week.Tuesday )
		when weekday ( Holidays.Day ) = 3 then value ( Enum.Week.Wednesday )
		when weekday ( Holidays.Day ) = 4 then value ( Enum.Week.Thursday )
