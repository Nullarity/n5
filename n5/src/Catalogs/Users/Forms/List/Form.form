      <queryText>select Users.Ref as Ref, Users.DeletionMark as DeletionMark, Users.Parent as Parent, Users.IsFolder as IsFolder, Users.Code as Code,	Users.Description as Description,
	Users.AccessDenied as AccessDenied, Users.AccessRevoked as AccessRevoked, Users.ApproveSalesOrders as ApproveSalesOrders, Users.ChangeInternalOrders as ChangeInternalOrders,
	Users.ChangeSalesOrders as ChangeSalesOrders, Users.CompanyAccess as CompanyAccess, Users.Email as Email, Users.Employee as Employee, Users.FirstName as FirstName,
	Users.FullName as FullName, Users.Language as Language, Users.LastName as LastName, Users.OrganizationAccess as OrganizationAccess, Users.TimeZone as TimeZone,	
	Users.TrackingDistance as TrackingDistance, Users.TrackingPeriodicity as TrackingPeriodicity,	Users.TrackingProvider as TrackingProvider, Users.TrackingTime as TrackingTime,
	Users.TrackLocation as TrackLocation, Users.UserClass as UserClass, Users.WarehouseAccess as WarehouseAccess, Users.Companies as Companies,	Users.Organizations as Organizations,
	Users.OrganizationsStates as OrganizationsStates, Users.Rights as Rights,	Users.Warehouses as Warehouses, Users.WarehousesStates as WarehousesStates, Users.Predefined as Predefined,
	Users.PredefinedDataName as PredefinedDataName, not ( Users.DeletionMark or Users.AccessDenied or Users.AccessRevoked ) as Active,
	TasksList.Progress as TaskProgress, TasksList.Completed as TaskCompleted, TasksList.Status as TaskStatus, TasksList.Task as TaskRef, TasksList.Observation as Observation
from Catalog.Users as Users
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as TasksList
	on TasksList.Source = Users.Ref
	and TasksList.User = &amp;Performer}</queryText>
