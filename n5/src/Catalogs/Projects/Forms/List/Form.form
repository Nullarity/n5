    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select allowed Projects.Ref as Ref, Projects.DataVersion as DataVersion, Projects.DeletionMark as DeletionMark, Projects.Predefined as Predefined,
	Projects.Owner as Owner, Projects.Parent as Parent, Projects.IsFolder as IsFolder, Projects.Code as Code, Projects.Description as Description,
	Projects.Amount as Amount, Projects.Pricing as Pricing, Projects.Completed as Completed, Projects.Duration as Duration,
	Projects.CompletionDate as CompletionDate, Projects.Cost as Cost, Projects.Currency as Currency,
	case when Projects.DateEnd = datetime ( 1, 1, 1 ) then datetime ( 3999, 12, 31 ) else Projects.DateEnd end as DateEnd,
	Projects.DateStart as DateStart, Projects.HourlyRate as HourlyRate, Projects.Minutes as Minutes, Projects.Manager as Manager,
	Projects.Memo as Memo, Projects.UseApprovingProcess as UseApprovingProcess, Projects.ApprovalList as ApprovalList,
	Projects.ObligatoryTasks as ObligatoryTasks,
	case when Projects.IsFolder or ( isnull ( AttachmentsCount.Count, 0 ) = 0 ) then 0 else 1 end as AttachmentPicture,
	case when Projects.IsFolder or Projects.Completed or Projects.DateEnd > &amp;CurrentDate or Projects.DateEnd = datetime ( 1, 1, 1 ) then false else true end as Overdue,
	case when isnull ( ProjectsCost.BillableMinutesTurnover + ProjectsCost.NonBillableMinutesTurnover, 0 )
				- isnull ( PlannedProjectsCost.BillableMinutesTurnover + PlannedProjectsCost.NonBillableMinutesTurnover, 0 ) &lt;=0 then 0
		else
			cast ( ( ( ProjectsCost.BillableMinutesTurnover + ProjectsCost.NonBillableMinutesTurnover
			- isnull ( PlannedProjectsCost.BillableMinutesTurnover + PlannedProjectsCost.NonBillableMinutesTurnover, 0 ) ) / 60 ) - 0.5 as Number ( 15, 0 ) )
			+ ( ( ProjectsCost.BillableMinutesTurnover + ProjectsCost.NonBillableMinutesTurnover
				- isnull ( PlannedProjectsCost.BillableMinutesTurnover + PlannedProjectsCost.NonBillableMinutesTurnover, 0 ) ) / 60
				- cast ( ( ( ProjectsCost.BillableMinutesTurnover + ProjectsCost.NonBillableMinutesTurnover
				- isnull ( PlannedProjectsCost.BillableMinutesTurnover + PlannedProjectsCost.NonBillableMinutesTurnover, 0 ) ) / 60 ) - 0.5 as Number ( 15, 0 ) ) ) * 0.6
	end as ActualHours,
	case when Projects.Minutes = 0 then 100 else 100 * ( ProjectsCost.BillableMinutesTurnover + ProjectsCost.NonBillableMinutesTurnover
		- isnull ( PlannedProjectsCost.BillableMinutesTurnover + PlannedProjectsCost.NonBillableMinutesTurnover, 0 ) ) / Projects.Minutes end as CompletionPercent,
	case when ProjectInvoices.Invoice is null then false else true end as Invoiced,
	ProjectTags.Tags as Tags,
	TasksList.Progress as TaskProgress, TasksList.Completed as TaskCompleted, TasksList.Status as TaskStatus, TasksList.Task as TaskRef, TasksList.Observation as Observation
from Catalog.Projects as Projects
	//
	// ProjectTags
	//
	left join InformationRegister.ProjectSuperTags as ProjectTags
	on ProjectTags.Project = Projects.Ref
	//
	// ProjectsCost
	//
	left join AccumulationRegister.ProjectsCost.Turnovers ( , , , {Project.Owner.* as Owner, Project.Completed as Completed, Project.*} ) as ProjectsCost
	on ProjectsCost.Project = Projects.Ref
	//
	// PlannedProjectsCost
	//
	left join AccumulationRegister.ProjectsCost.Turnovers ( &amp;CurrentDate, , , {Project.Owner.* as Owner, Project.Completed as Completed, Project.*} ) as PlannedProjectsCost
	on PlannedProjectsCost.Project = Projects.Ref
	//
	// AttachmentsCount
	//
	left join InformationRegister.AttachmentsCount as AttachmentsCount
	on AttachmentsCount.Reference = Projects.Ref
	//
	// ProjectInvoices
	//
	{left join InformationRegister.ProjectInvoices as ProjectInvoices
	on ProjectInvoices.Project = Projects.Ref
	and ProjectInvoices.Invoice &lt;> value ( Document.ProjectsInvoice.EmptyRef )}
	//
	// Employee
	//
	{join ( select distinct EmployeeTasks.Ref as Ref, EmployeeTasks.Employee as Employee
			from Catalog.Projects.Tasks as EmployeeTasks ) as EmployeeTasks
	on EmployeeTasks.Ref = Projects.Ref}
	//
	// Tasks
	//
	{left join InformationRegister.Tasks as TasksList
	on TasksList.Source = Projects.Ref
	and TasksList.User = &amp;Performer}
	//
	// Filter by Tags
	//
	{join InformationRegister.ProjectTags as TagsFilter
	on TagsFilter.Project = Projects.Ref}
{where EmployeeTasks.Employee = &amp;Employee, TagsFilter.Tag.* as Tag}
</queryText>
