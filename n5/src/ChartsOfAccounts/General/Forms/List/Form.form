    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Accounts.Ref, Accounts.DeletionMark, Accounts.Parent, Accounts.Code, Accounts.Order,
	Accounts.Type, Accounts.OffBalance, Accounts.Class, Accounts.Quantitative, Accounts.Currency as CurrencyFlag,
	Accounts.Predefined, Accounts.PredefinedDataName, Balances.AmountBalance as Balance,
	presentation ( Types1.ExtDimensionType ) as Dim1, presentation ( Types2.ExtDimensionType ) as Dim2,
	presentation ( Types3.ExtDimensionType ) as Dim3, Accounts.Offline as Offline,
	Accounts.Folder as Folder, Accounts.DescriptionRo as DescriptionRo, Accounts.DescriptionRu as DescriptionRu,
	case when Accounts.DeletionMark then 4 else 0 end
	+ case when Accounts.Folder then
			case when Accounts.Predefined then 0 else 1 end
		else case when Accounts.Predefined then 2 else 3 end
	end as Picture
from ChartOfAccounts.General as Accounts
	//
	// Balances
	//
	{left join AccountingRegister.General.Balance ( , , , {Company.* as Company} ) as Balances
	on Balances.Account = Accounts.Ref}
	//
	// Types1
	//
	left join ChartOfAccounts.General.ExtDimensionTypes as Types1
	on Types1.Ref = Accounts.Ref
	and Types1.LineNumber = 1
	//
	// Types2
	//
	left join ChartOfAccounts.General.ExtDimensionTypes as Types2
	on Types2.Ref = Accounts.Ref
	and Types2.LineNumber = 2
	//
	// Types3
	//
	left join ChartOfAccounts.General.ExtDimensionTypes as Types3
	on Types3.Ref = Accounts.Ref
	and Types3.LineNumber = 3
</queryText>
      <mainTable>ChartOfAccounts.General</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
